<<widget "undergroundRobinTopic">>
	<<if $undergroundbrothel.robin>>
		<<set $_state to _args[1] ? _args[1] : "discussed">>
		<<set $undergroundbrothel.robinTopics[_args[0]] to $_state>>
	<</if>>
<</widget>>

<<widget "undergroundEscapeForestRobin">>

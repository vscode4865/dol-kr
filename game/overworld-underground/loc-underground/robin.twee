		<<set $undergroundbrothel.robinTopics to {
			"cold": "undiscussed",
			"lonely": "undiscussed",
			"dark": "undiscussed",
			"scared": "undiscussed",
			"touch": "undiscussed"
		}>>
	<</if>>
<</widget>>


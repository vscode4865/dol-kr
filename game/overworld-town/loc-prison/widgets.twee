			<<saveNPC $_id "scarred_inmate">>
		<</if>>
	<<else>>
		<<generateRole 4 "scarred" "inmate">>
		<<saveNPC 4 "scarred_inmate">>
	<</if>>
<</widget>>

<<widget "generate_tattooed_inmate">>

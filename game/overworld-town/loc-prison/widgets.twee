<<widget "prison_list_result">>
	<<switch $prison_punishment_number>>
		<<case 2>>
			<span class="red">고행</span><<set $prison_punishment to "free use">>
		<<case 3>>
			<span class="pink">심판</span><<set $prison_punishment to "nude">>
		<<case 4>>
			<span class="purple">불견</span><<set $prison_punishment to "blindfold">>
		<<case 5>>
			<span class="blue">헌신</span><<set $prison_punishment to "topless">>
		<<case 6>>

					<</switch>>
				</tr>
			<</if>>
			<tr>
				<<if not _mobile>>
					<th rowspan="2" style="border-bottom-width:2px" valign="middle" width="40%" align="left">물건</th>
				<</if>>
				<<switch _canmath>>
					<<case 3>>
						<th colspan="2" align="center">소지품</th>
					<<case 4 5 6>>

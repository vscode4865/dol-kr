			<label><span class="meek"><<trClothes "under_lower" $worn.under_lower.name "name" "을">>_trResult 벗는다</span> <<combatexhibitionist3>> <<radiobutton "$danceaction" "understripskirt">></label> |
			<</if>>
		<<elseif $worn.lower.type.includes("naked")>>
			<<if $exhibitionism gte 75>>
			<label><span class="meek"><<trClothes "under_lower" $worn.under_lower.name "name" "을">>_trResult 벗는다</span> <<combatexhibitionist5>> <<radiobutton "$danceaction" "understripnude">></label> |
			<</if>>
		<</if>>
	<</if>>
<</if>>
<</widget>>

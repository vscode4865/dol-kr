				<</if>>
			<</if>>
		<</if>>

		<<if !$worn.under_upper.type.includes("naked") and _underOutfit is false>>
			<<if !$worn.upper.type.includes("naked") and $worn.upper.open is 1>>
				<label><span class="meek"><<trClothes "under_upper" $worn.under_upper.name "name" "을">>_trResult 벗는다</span> <<combatexhibitionist1>> <<radiobutton "$danceaction" "underupperstripopen">></label> |
			<<elseif $player.breastsize gte 3>>
				<<if hasSexStat("exhibitionism", 4)>>
				<label><span class="meek"><<trClothes "under_upper" $worn.under_upper.name "name" "을">>_trResult 벗는다</span> <<combatexhibitionist4>> <<radiobutton "$danceaction" "underupperstripbreasts">></label> |
				<</if>>
			<<elseif $player.gender_appearance isnot "m" and $worn.under_lower.gender isnot "m">>
				<<if hasSexStat("exhibitionism", 4)>>
				<label><span class="meek"><<trClothes "under_upper" $worn.under_upper.name "name" "을">>_trResult 벗는다</span> <<combatexhibitionist4>> <<radiobutton "$danceaction" "underupperstripfchest">></label> |
				<</if>>
			<<else>>
			<label><span class="meek"><<trClothes "under_upper" $worn.under_upper.name "name" "을">>_trResult 벗는다</span> <<combatexhibitionist1>> <<radiobutton "$danceaction" "underupperstripmchest">></label> |
			<</if>>
		<</if>>

		<<if !$worn.under_lower.type.includes("naked") and _underOutfit is false>>
			<<if !$worn.lower.type.includes("naked") and setup.clothes.lower[clothesIndex('lower', $worn.lower)].skirt is 1>>
				<<if hasSexStat("exhibitionism", 3)>>
				<label><span class="meek"><<trClothes "under_lower" $worn.under_lower.name "name" "을">>_trResult 벗는다</span> <<combatexhibitionist3>> <<radiobutton "$danceaction" "understripskirt">></label> |
				<</if>>
			<<elseif $worn.lower.type.includes("naked")>>
				<<if hasSexStat("exhibitionism", 5)>>
				<label><span class="meek"><<trClothes "under_lower" $worn.under_lower.name "name" "을">>_trResult 벗는다</span> <<combatexhibitionist5>> <<radiobutton "$danceaction" "understripnude">></label> |
				<</if>>
			<</if>>
		<</if>>
	<</if>>
<</widget>>

<<widget "dance_stage_cum">>
	<<if $dance_stage_cum is undefined>>
		<<set $dance_stage_cum to 0>>
	<</if>>
	<<if _args[0]>>
		<<set $dance_stage_cum += _args[0]>>
	<</if>>
<</widget>>

<<widget "dance_private_init">>
	<!--
	<<for _i to 0; _i lt $NPCList.length; _i++>>
		<<if $dance_place isnot _i>>
			<<deactivateNPC _i>>
		<</if>>
	<</for>>
	-->
	<<if $dance_place isnot 0>>
		<<saveNPC $dance_place chosen_npc>>
		<<saveNPC 0 zero_npc>>
		<<clearsinglenpc $dance_place>>
		<<clearsinglenpc 0>>
		<<loadNPC 0 chosen_npc>>
		<<loadNPC $dance_place zero_npc>>
		<<clearNPC zero_npc>>
		<<clearNPC chosen_npc>>
	<</if>>
	<<set $enemyno to 1>>
	<<unset $corruptionDancing>>
	<<set $phase to 0>>
	<<person1>>
<</widget>>
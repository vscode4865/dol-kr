		<label><span class="meek"><<trClothes "under_upper" $worn.under_upper.name "name" "을">>_trResult 벗는다</span> <<combatexhibitionist1>> <<radiobutton "$danceaction" "underupperstripmchest">></label> |
		<</if>>
	<</if>>

	<<if !$worn.under_lower.type.includes("naked") and _underOutfit is false>>
		<<if !$worn.lower.type.includes("naked") and setup.clothes.lower[clothesIndex('lower', $worn.lower)].skirt is 1>>

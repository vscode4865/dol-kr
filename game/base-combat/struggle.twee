	<<else>>
		<<set $mouthtarget to "struggle">>
		<<switch $mouthuse>>
			<<case "struggle">>
				<span @class="($lastOptions.mouth isnot _mouthOptions or _mouthGold is true ?'gold':'')"><<struggle_mouth_creature_ ga>> 당신의 얼굴에 달라붙어 있다.</span>
				<<if $leftarmstate is "mouth_struggle" and $rightarmstate is "mouth_struggle">>
					당신은 그것을 양손으로 잡고 있다.
				<<elseif $leftarmstate is "mouth_struggle">>

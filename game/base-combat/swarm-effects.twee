			<</if>>

			<<if _swarmamounts.genital[0] lte 0 and _swarmamounts.genital[1] lte 0>>
				<label>당신의 <<genitals_rul>> 가린다 <<radiobutton "$leftaction" "frontcoverleft">></label> |
			<</if>>

			<<if _swarmamounts.butt[0] lte 0 and _swarmamounts.butt[1] lte 0>>
				<label>당신의 엉덩이를 가린다 <<radiobutton "$leftaction" "backcoverleft" checked>></label> |
			<</if>>

			<<if _swarmamounts.chest lte 0>>
				<label>당신의 가슴을 가린다 <<radiobutton "$leftaction" "chestcoverleft">></label> |
			<</if>>

			<<if _swarmamounts.genital[1] gte 1 and !$worn.genitals.type.includes("chastity")>>
				<label><span class="brat">그 <<swarmtype_ul>> 당신의 <<genitalsPost>>에서 빼낸다</span> <<radiobutton "$leftaction" "frontpurgeleft">></label> |
			<</if>>

			<<if _swarmamounts.genital[0] gte 1 and !$worn.genitals.type.includes("chastity")>>
				<label><span class="brat">그 <<swarmtype_ul>> 당신의 <<genitalsPost>>에 가까이 오지 못하게 한다</span> <<radiobutton "$leftaction" "frontclearleft">></label> |
			<</if>>

			<<if _swarmamounts.butt[1] gte 1 and $worn.genitals.anal_shield isnot 1>>
				<label><span class="brat">그 <<swarmtype_ul>> 당신의 항문에서 빼낸다</span> <<radiobutton "$leftaction" "backpurgeleft" checked>></label> |
			<</if>>

			<<if _swarmamounts.butt[0] gte 1 and $worn.genitals.anal_shield isnot 1>>
				<label><span class="brat">그 <<swarmtype_ul>> 당신의 엉덩이에 가까이 오지 못하게 한다</span> <<radiobutton "$leftaction" "backclearleft" checked>></label> |
			<</if>>


					<<case "healthy">><<set $_text_output to $_stringsPre + `"너 자신을 잘 가꿨구나. 좋아."`>>
					<<case "default">><<set $_text_output to $_stringsPre + `"일을 꼭 그래야 하는 것 보다 더 어렵게 만들지 마라."`>>
				<</switch>>
			<<elseif $rng gte 60>>
				<<set $_text_output to $_stringsPre + `"너는 정말 비싼 값이 붙겠어!"`>>
			<<elseif $rng gte 40>>

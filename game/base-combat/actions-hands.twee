			<<elseif $vaginause is 0>>
				<<set _leftaction["보지를 가린다"] to "leftcovervagina">>
			<</if>>
		<</if>>
	<</if>>

	<<if ($worn.lower.vagina_exposed is 1 and $worn.under_lower.vagina_exposed is 1) or $leftactiondefault is "leftcoverpenislewd">>
		<<trClothes "genitals" $worn.genitals.name "name" "을">><<set _penis to playerHasStrapon() ? "페니스 밴드 자지를" : (playerChastity("penis") ? _trResult : "자지를")>>

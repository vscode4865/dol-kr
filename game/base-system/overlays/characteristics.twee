:: Characteristics [widget]

<<widget "characteristics">>
	<br>
	당신은
	<<if $wraithSkin>>
		유령 같은
	<<elseif $wolfgirl gte 6 or $cat gte 6 or $cow gte 6 or $harpy gte 6 or $fox gte 6>>
		<<if $angel gte 6>>
			천사 같은
		<<elseif $fallenangel gte 2>>
			타락한
		<<elseif $demon gte 6>>
			악마 같은
		<<else>>
			
		<</if>>
		<<if $wolfgirl gte 6 and ($demon gte 6 or $fallenangel gte 2)>>
			지옥견
		<<elseif $wolfgirl gte 6 and $angel gte 6>>
			개
		<<elseif $wolfgirl gte 6>>
			늑대
		<<elseif $cat gte 6>>
			고양이
		<<elseif $cow gte 6>>
			<<if $player.gender_appearance is "m">>
				황소
			<<else>>
				젖소
			<</if>>
		<<elseif $harpy gte 6>>
			<<if $hallucinations gte 2 or $bestialitydisable is "t">>
				하피
			<<else>>
				새
			<</if>>
		<<elseif $fox gte 6>>
			여우
		<</if>>
	<<else>>
		<<if $angel gte 6>>
			천사
		<<elseif $fallenangel gte 2>>
			타천사
		<<elseif $demon gte 6>>
			악마
		<<else>>
			/*a<<if $NudeGenderDC lt 0 and $player.gender_posture is "n">>n<</if>> 한국어에는 관사 없음*/
		<</if>>
	<</if>>
	<<if $NudeGenderDC gte 0>>
		<<genderPost "이다">>.
	<<else>>
		<<gender_posture>>.
	<</if>>
	<br><br>

	<<switch $bodysize>>
		<<case 0>>당신은 또래보다 몸집이 왜소하다.
		<<case 1>>당신은 또래보다 몸집이 작다.
		<<case 2>>당신은 또래와 몸집이 비슷하다.
		<<case 3>>당신은 또래보다 몸집이 크다.
	<</switch>>

	<!-- Eye details -->
	<<print window.buildEyeDetails()>>
	<!-- -->

	당신의 피부는 <<skincolourtext>>
	<<if $player.breastsize gte 1>>
		<<if $player.breastsize is $player.perceived_breastsize>>
			당신은 <<breasts_ rul>> 가지고 있다.
		<<else>>
			당신은 <<breasts_ rul>> 가지고 있지만, 당신의
			<<if $player.perceived_breastsize is 0>>
				옷이 가리고 있어, 평평한 듯이 보인다.
			<<elseif $player.perceived_breastsize lt $player.breastsize>>
				옷이 가리고 있어, 사람들에게 <<breastsizedesc $player.perceived_breastsize>> 가슴으로 보인다.
			<<else>> /*if $player.perceived_breastsize gt $player.breastsize*/
				옷에 있는 패드 때문에, 사람들에게 <<breastsizedesc $player.perceived_breastsize>> 가슴으로 보인다.
			<</if>>
		<</if>>
	<<elseif $player.breastsize is 0 and $player.perceived_breastsize gte 1>>
		당신의 가슴은 평평하지만, 당신의 옷에 있는 패드 때문에, 사람들에게 <<breastsizedesc $player.perceived_breastsize>> 가슴으로 보인다.
	<</if>>
	당신은 <<bottom_ ul true>> 가지고 있다.
	<<if $player.penisExist and $player.vaginaExist>>
		<<silently>>
			<<penisPost false true "과">><<set _penis to _trResult>>
			<<pussyPost false true "을">><<set _pussy to _trResult>>
		<</silently>>
		당신은 <<print _penis>> <<print _pussy>> 가지고 있다.
	<<else>>
		당신은
		<<if $player.penisExist>>
			<<silently>><<penisPost false true "을">><<set _penis to _trResult>><</silently>>
			<<print _penis>> 가지고 있다.
		<</if>>
		<<if $player.vaginaExist>>
			<<silently>><<pussyPost false true "을">><<set _pussy to _trResult>><</silently>>
			<<print _pussy>> 가지고 있다.
		<</if>>
	<</if>>

	<!--Hair Details-->
	<<set _hairTextConfig = { currentValue : $hairlength, states : [
			{ requiredValue: 0,		color: '',	preText: '당신의 ',									description: '<<haircolourtext>>', postText: ' 머리카락은 짧다.' },
			{ requiredValue: 100,	color: '',	preText: '자연스럽게 둘 때, 당신의 ',				description: '<<haircolourtext>>', postText: ' 머리카락은 턱까지 내려오고, <<fringedescription>>' },
			{ requiredValue: 200,	color: '',	preText: '자연스럽게 둘 때, 당신의 ',				description: '<<haircolourtext>>', postText: ' 머리카락은 어깨까지 내려오고, <<fringedescription>>' },
			{ requiredValue: 300,	color: '',	preText: '자연스럽게 둘 때, 당신의 ',				description: '<<haircolourtext>>', postText: ' 머리카락은 꽤나 길어서, 가슴 바로 위까지 내려오며, <<fringedescription>>' },
			{ requiredValue: 400,	color: '',	preText: '자연스럽게 둘 때, 당신의 기다란 ',			description: '<<haircolourtext>>', postText: ' 머리카락은 유두까지 내려오고, <<fringedescription>>' },
			{ requiredValue: 500,	color: '',	preText: '자연스럽게 둘 때, 당신의 기다란 ',			description: '<<haircolourtext>>', postText: ' 머리카락은 배 위까지 내려오고, <<fringedescription>>' },
			{ requiredValue: 600,	color: '',	preText: '자연스럽게 둘 때, 당신의 기다란 ',			description: '<<haircolourtext>>', postText: ' 머리카락은 배꼽까지 내려오고, <<fringedescription>>' },
			{ requiredValue: 700,	color: '',	preText: '자연스럽게 둘 때, 당신의 매우 기다란 ',		description: '<<haircolourtext>>', postText: ' 머리카락은 허벅지까지 내려오고, <<fringedescription>>' },
			{ requiredValue: 800,	color: '',	preText: '자연스럽게 둘 때, 당신의 매우 기다란 ',		description: '<<haircolourtext>>', postText: ' 머리카락은 무릎까지 내려오고, <<fringedescription>>' },
			{ requiredValue: 900,	color: '',	preText: '자연스럽게 둘 때, 당신의 지나칠 정도로 기다란 ',	description: '<<haircolourtext>>', postText: ' 머리카락은 발목까지 내려오고, <<fringedescription>>' },
			{ requiredValue: 1000,	color: '',	preText: '자연스럽게 둘 때, 당신의 ',				description: '<<haircolourtext>>', postText: ' 머리카락이 너무 길어 걸려 넘어지지 않도록 조심해야 할 정도며, <<fringedescription>>' }
		]
	}>>
	<<characteristic-text _hairTextConfig>>
	<br><br>

	<!--Pregnancy Details-->
	<<set $_pregnancy to getPregnancyObject()>>
	<<set $_menstruation to $sexStats.vagina.menstruation>>
	<<set $_pills to $sexStats.pills>>
	<<set $_knowsTheyCanBecomePregnant to ($playerPregnancyHumanDisable is "f" or $playerPregnancyBeastDisable is "f") and playerAwareTheyCanBePregnant()>>
	<<set $_heatArousal to playerHeatMinArousal()>>
	<<set $_rutArousal to playerRutMinArousal()>>
	<<set $_heatRutDisplay to ($_heatArousal and playerAwareTheyAreInHeat() ? "임신하기" : "") + ($_heatArousal and playerAwareTheyAreInHeat() and $_rutArousal ? " 쉽고 " : " 쉬운") + ($_rutArousal ? "발정하기 쉬운" : "")>>
	<<if $_pregnancy.fetus.length and $_pregnancy.awareOf>>
		<<if $_pregnancy.timer / $_pregnancy.timerEnd lte 0.33>>
			<<set _trimester to "초기">>
		<<elseif $_pregnancy.timer / $_pregnancy.timerEnd lte 0.66>>
			<<set _trimester to "중기">>
		<<else>>
			<<set _trimester to "후기">>
		<</if>>
		<<if $_pregnancy.awareOfDetails || $_pregnancy.potentialFathers.every((father, index, array) => father.type === array[0].type)>>
			<<if $_pregnancy.awareOfDetails>>
				<<set $_number to $_pregnancy.fetus.length>>
			<<elseif $_pregnancy.awareOfMultiple>>
				<<set $_number to $_pregnancy.fetus.length is 1 ? 1 : "more than one">>
			<<else>>
				<<set $_number to "an unknown number of">>
			<</if>>
			<<switch $_pregnancy.type>>
				<<case "human">>
					<<if $_number isnot "an unknown number of" and $_number isnot "more than one" and $_number gt 1>>
						<<set _childType to "">>
						<<if $_pregnancy.fetus[0].features.identical is true and $_pregnancy.awareOfDetails>>
							<<set _childType += "일란성 ">>
						<</if>>
						<<set _childType += ($_number is 3 ? "세쌍둥이를" : "쌍둥이를")>>
					<<else>>
						<<set _childType to ($_number is 1 || $_number is "more than one" ? "아기를" : "아기들을")>>
					<</if>>
				<<case "wolf">>
					<<set _childType to ($_number is 1 || $_number is "more than one" ? "늑대 강아지를" : "늑대 강아지들을")>>
				<<case "hawk">>
					<<set _childType to ($_number is 1 || $_number is "more than one" ? "매의 알을" : "매의 알들을")>>
			<</switch>>
			<<if $_pregnancy.type is "hawk">>
				당신은 <<trNumber $_number>> _childType 임신하고 있으며,
				<<switch _trimester>>
					<<case "first">>알을 낳기까지 시간이 꽤 걸릴 것이다.
					<<case "second">>알을 낳기까지 아직 어느 정도 시간이 걸릴 것이다.
					<<case "third">>곧 알을 낳을 수 있게 될 것이다.
				<</switch>>
			<<else>>
				당신은 <<if !["쌍둥이", "일란성 쌍둥이", "세쌍둥이"].includes(_childType)>><<trNumber $_number>><</if>> _childType 임신하고 있으며, 현재 임신 _trimester 이다.
			<</if>>
		<<else>>
			당신은 당신이 현재 임신 _trimester라는 것을 알고 있다.
		<</if>>
		<<if between($_pregnancy.timer / $_pregnancy.timerEnd, 0.1, 1) and !$daily.ultrasoundDone>>
			<span class="blue">당신은 병원에서 초음파 검사를 받을 수 있다.</span>
		<</if>>
		<<if $_pregnancy.potentialFathers.length is 1>>
			당신은 아기 아빠가 <<print pregnancyNameCorrection($_pregnancy.potentialFathers[0].source)>>?ira는 것을 안다.
		<<elseif $_pregnancy.potentialFathers.length is 2>>
			당신은 아기 아빠가 <<print pregnancyNameCorrection($_pregnancy.potentialFathers[0].source)>>?na <<print pregnancyNameCorrection($_pregnancy.potentialFathers[1].source)>>?irago 확신한다.
		<<elseif $_pregnancy.potentialFathers.length is 3>>
			당신은 아기 아빠가 <<print pregnancyNameCorrection($_pregnancy.potentialFathers[0].source)>>?na, <<print pregnancyNameCorrection($_pregnancy.potentialFathers[1].source)>>?na 아니면 <<print pregnancyNameCorrection($_pregnancy.potentialFathers[2].source)>>?irago 거의 확신한다.
		<<else>>
			당신은 아기 아빠가 누군지 확신하지 못한다.
		<</if>>
		<br><br>
	<<elseif $_knowsTheyCanBecomePregnant and $_pills.pills.contraceptive.doseTaken>>
		<<if $_pills.pills.contraceptive.doseTaken gte 2>>
			당신은 피임약을 과다 복용해서, 당신의 임신 확률을 0으로 낮춰 두었다.
		<<else>>
			당신은 피임약을 복용해서, 당신의 임신 확률을 거의 0으로 낮춰 두었다.
		<</if>>
		<br><br>
	<<elseif $_knowsTheyCanBecomePregnant and (($sciencetrait gte 4 and $awareness gte 200) or $awareness gte 500)>>
		<<set $_risk to playerPregnancyRisk()>>
		<<if $_risk isnot undefined>>
			<<switch $_risk>>
				<<case 6>><<set $_pregnancyRisk to "매우 안전할">><<set $_pregnancyRiskColour to "green">>
				<<case 5>><<set $_pregnancyRisk to "안전할">><<set $_pregnancyRiskColour to "teal">>
				<<case 3 4>><<set $_pregnancyRisk to "어느-정도 안전할">><<set $_pregnancyRiskColour to "lblue">>
				<<case 1 2>><<set $_pregnancyRisk to "위험할">><<set $_pregnancyRiskColour to "pink">>
				<<case 0>><<set $_pregnancyRisk to "매우 위험할">><<set $_pregnancyRiskColour to "red">>
			<</switch>>

			<<if $_heatArousal or $_rutArousal>>
				<<heatRutDisplay>>
				성행위는 <span @class="$_pregnancyRiskColour">$_pregnancyRisk</span> 것이다.
			<<else>>
				당신은 성행위가 <span @class="$_pregnancyRiskColour">$_pregnancyRisk</span> 것 같다는 기분이 든다.
			<</if>>
			<br><br>
		<</if>>
	<<else>>
		<<heatRutDisplay true>>
	<</if>>

	<!--Semen Details-->
	<<if $player.penisExist and $player.penissize gte -1>>
		<<set _semenCapacityTextConfig = { currentValue : $semen_volume, preText: '당신은 ', states : [
				{ requiredValue: 0,		color: '',			description: '',				postText: ' 정액을 생산하지 않는다.' },
				{ requiredValue: 1,		color: 'red',		description: '아주 적은',			postText: ' 양의 정액을 ' },
				{ requiredValue: 100,	color: 'pink',		description: '적은',			postText: ' 양의 정액을 ' },
				{ requiredValue: 300,	color: 'purple',	description: '많지는 않은',		postText: ' 양의 정액을 ' },
				{ requiredValue: 500,	color: 'blue',		description: '적절한',		postText: ' 양의 정액을 ' },
				{ requiredValue: 1000,	color: 'lblue',		description: '많은',			postText: ' 양의 정액을 ' },
				{ requiredValue: 1500,	color: 'teal',		description: '막대한',			postText: ' 양의 정액을 ' },
				{ requiredValue: 2000,	color: 'green',		description: '놀랄 만한',	postText: ' 양의 정액을 ' }
			]
		}>>
		<<set _semenVolumeTextConfig = { currentValue : $semen_amount, states : [
				{ requiredValue: 0,							color: 'red',		preText: "생산하나, ",				description: "바짝 마를 때까지 쥐어짜여 있다."},
				{ requiredValue: 1,							color: 'pink',		preText: "생산하나, ", 	description: '거의 비어있다. '},
				{ requiredValue: ($semen_volume / 7) * 2,	color: 'pink',		preText: "생산하나, ",	description: '얼마 안 남아있다. '},
				{ requiredValue: ($semen_volume / 7) * 3,	color: 'purple',	preText: "생산하나, ",	description: '꽤 양이 줄어있다. '},
				{ requiredValue: ($semen_volume / 7) * 4,	color: 'blue',		preText: "생산하며,  ",	description: '반쯤 쌓여있다. '},
				{ requiredValue: ($semen_volume / 7) * 5,	color: 'lblue',		preText: "생산하며,  ",	description: '많이 쌓여있다. '},
				{ requiredValue: ($semen_volume / 7) * 6,	color: 'teal',		preText: "생산하며,  ",	description: '거의 가득 쌓여있다. '},
				{ requiredValue: $semen_volume,				color: 'green',		preText: "생산하며,  ",	description: '최대 한도로 쌓여있다. '}
			]
		}>>
		<<characteristic-text _semenCapacityTextConfig>>
		<<if $semen_volume gt 0>>
			<<characteristic-text _semenVolumeTextConfig>>
			<<if $semen_amount gte $semen_volume>>
				<<if $arousal gte ($arousalmax / 5) * 4>>
					이미 가득 쌓인 상태에서, <span class="pink">흥분으로 용량이 증가한다.</span>
				<<elseif $arousal gte ($arousalmax / 5) * 3>>
					이미 가득 쌓인 상태에서, <span class="purple">흥분으로 용량이 증가한다.</span>
				<</if>>
			<<else>>
				<<if $arousal gte ($arousalmax / 5) * 4>>
					<span class="pink">흥분한 것이 회복하는 데에 도움을 준다.</span>
				<<elseif $arousal gte ($arousalmax / 5) * 3>>
					<span class="purple">흥분한 것이 회복하는 데에 도움을 준다.</span>
				<</if>>
			<</if>>
		<</if>>
		<br>
	<</if>>

	<!--Lactation Details-->
	<<if $lactating is 1 and $breastfeedingdisable is "f">>
		<<set _milkCapacityTextConfig = { currentValue : $milk_volume, preText: '당신의 <<breasts_ nun>> ', postText: ' 양의 모유를 ', states : [
					{ requiredValue: 0,		color: 'red',		description: '아주 적은'},
					{ requiredValue: 100,	color: 'pink',		description: '적은'},
					{ requiredValue: 300,	color: 'purple',	description: '많지는 않은'},
					{ requiredValue: 500,	color: 'blue',		description: '적절한'},
					{ requiredValue: 1000,	color: 'lblue',		description: '많은'},
					{ requiredValue: 1500,	color: 'teal',		description: '막대한'},
					{ requiredValue: 2000,	color: 'green',		description: '놀랄 만한'}
				]
			}>>
		<<set _milkVolumeTextConfig = { currentValue : $milk_amount, states : [
				{ requiredValue: 0,							color: 'red',		preText: "생산하나, ",				description: " 바짝 마를 때까지 쥐어짜여 있다."},
				{ requiredValue: 1,							color: 'pink',		preText: "생산하나, ", 	description: ' 거의 비어있다.'},
				{ requiredValue: ($milk_volume / 7) * 2,	color: 'pink',		preText: "생산하나, ",	description: ' 얼마 안 남아있다.'},
				{ requiredValue: ($milk_volume / 7) * 3,	color: 'purple',	preText: "생산하나, ",	description: ' 꽤 양이 줄어있다.'},
				{ requiredValue: ($milk_volume / 7) * 4,	color: 'blue',		preText: "생산하며, ",	description: ' 반쯤 쌓여있다.'},
				{ requiredValue: ($milk_volume / 7) * 5,	color: 'lblue',		preText: "생산하며, ",	description: ' 많이 쌓여있다.'},
				{ requiredValue: ($milk_volume / 7) * 6,	color: 'teal',		preText: "생산하며, ",	description: ' 거의 가득 쌓여있다.'},
				{ requiredValue: $milk_volume,				color: 'green',		preText: "생산하며, ",	description: ' 최대 한도로 쌓여있다.'}
			]
		}>>
		<<characteristic-text _milkCapacityTextConfig>>
		<<characteristic-text _milkVolumeTextConfig>>
		<<if $milkFullPain and $milkFullPain gt 200>>
			모유가 가득한 상태로 너무 오래 있어서 당신은 아픔을 느낀다.
			<<if !$player.inventory.sextoys["breast pump"] and $milkFullPain gt 225>><span class="gold">약국에서 파는 착유기가 도움이 될 수 있을 것이다.</span><</if>>
		<</if>>
		<br>
	<</if>>

	<<set _holeWetnessStates = [
			{ requiredValue: 0,		color: 'green',	description: '말라있다.',			postText: ' 여기에 뭘 넣기는 힘들 것이다.' },
			{ requiredValue: 15,	color: 'teal',	description: '살짝 매끈하다.',	postText: ' 제아무리 작은 자지라도 쉽사리 들어가진 않을 것이다.' },
			{ requiredValue: 30,	color: 'lblue',	description: '매끈하다.',			postText: ' 대부분의 자지가 잘 들어가지 않을 것이다.' },
			{ requiredValue: 45,	color: 'blue',	description: '축축하다.',			postText: ' 어떤 자지는 별다른 어려움 없이 들어갈 것이다.' },
			{ requiredValue: 60,	color: 'purple',description: '젖어있다.',			postText: ' 대부분의 자지는 쉽사리 들어갈 것이다.' },
			{ requiredValue: 75,	color: 'pink',	description: '물이 뚝뚝 떨어진다.',		postText: ' 제아무리 큰 자지라도 쉽사리 들어갈 것이다.' },
			{ requiredValue: 90,	color: 'red',	description: '흠뻑 젖어있다.',		postText: ' 여기에 들어가지 못할 자지는 없다.' }
	]>>
	<!--Vagina Wetness-->
	<<if $player.vaginaExist>>
		<<set _vaginaWetnessTextConfig = { currentValue : $vaginaWetness, preText: '당신의 보지는 ', states: _holeWetnessStates}>>
		<<characteristic-text _vaginaWetnessTextConfig>>
		<br>
	<</if>>

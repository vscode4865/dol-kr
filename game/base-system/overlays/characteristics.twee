			{ requiredValue: 30,	color: 'lblue',	description: '매끈하다.',			postText: ''},
			{ requiredValue: 45,	color: 'blue',	description: '축축하다.',			postText: ''},
			{ requiredValue: 60,	color: 'purple',description: '젖어있다.',			postText: ''},
			{ requiredValue: 75,	color: 'pink',	description: '물이 뚝뚝 떨어진다.',		postText: ''},
			{ requiredValue: 90,	color: 'red',	description: '흠뻑 젖어있다.',	postText: ($player.penissize >= 4) ? " 이 정도 크기라도, 어디에 넣기 어렵진 않을 것이다." : ''}
		]}>>
		<<characteristic-text _penisWetnessTextConfig>>
		<br>
	<</if>>
	<!--Anal Wetness-->
	<<if $analdisable is "f">>
		<<set _analWetnessTextConfig = { currentValue : $anusWetness, preText: '당신의 엉덩이는 ', states: _holeWetnessStates}>>
		<<characteristic-text _analWetnessTextConfig>>
	<</if>>

	<br><br>
	<<bodywriting>>
	<div id="characteristics-display">

		<br>
		<span class="gold">주요 특성</span>
		<div id="base-characteristics" class="characteristic-box-list">
			<!--Core skill configurations-->
			<<silently>>

				<!--PURITY-->
				<<if $demon gte 6>>
					<<set _purityIcon='misc/icon/tf_demon'>>
				<<elseif $fallenangel gte 2>>
					<<set _purityIcon='misc/icon/tf_fallen'>>
				<<else>>
					<<set _purityIcon='misc/icon/tf_angel'>>
				<</if>>
				<<set _purityConfig = { name : "순결성", icon : _purityIcon, displayType : "level", currentValue : $purity, modifier: 100, modTypes: { good: [], bad: [] },
					states : [
						{ requiredValue: 0,		level: 0, color: 'red',		description: '당신은 더 이상 더럽혀지지 못할 정도로 더럽혀져 있다.'},
						{ requiredValue: 1,		level: 1, color: 'red',		description: '당신은 완전히 더럽혀졌다고 느낀다.'},
						{ requiredValue: 500,	level: 2, color: 'pink',	description: '당신은 더럽혀졌다고 느낀다.'},
						{ requiredValue: 600,	level: 3, color: 'purple',	description: '당신은 약간 더럽혀졌다고 느낀다.'},
						{ requiredValue: 700,	level: 4, color: 'blue',	description: '당신은 때묻었다고 느낀다.'},
						{ requiredValue: 800,	level: 5, color: 'lblue',	description: '당신은 약간 때묻었다고 느낀다.'},
						{ requiredValue: 900,	level: 6, color: 'teal',	description: "당신은 그렇게까지 순수하다고 느끼지 못한다."},
						{ requiredValue: 1000,	level: 7, color: 'green',	description: '당신은 천사 같다.'}
					]
				}>>

				<!--AWARENESS-->
				<<set _awarenessConfig = { name : "성지식", icon : "ui/sym_awareness", displayType : "level", currentValue : $awareness, modifier: 100, modTypes: { good: [], bad: [] },
					states : [
						{ requiredValue: -200,	level: 0, color: 'green',	description: '당신은 순수하다.'},
						{ requiredValue: 1,		level: 1, color: 'teal',	description: '당신은 거의 완전히 순수하다.'},
						{ requiredValue: 100,	level: 2, color: 'lblue',	description: '당신은 제한된 성지식을 갖고 있다.'},
						{ requiredValue: 200,	level: 3, color: 'blue',	description: '당신은 평범한 성지식을 갖고 있다.'},
						{ requiredValue: 300,	level: 4, color: 'purple',	description: '당신의 성지식은 대다수의 사람들을 넘어서 있다.'},
						{ requiredValue: 400,	level: 5, color: 'pink',	description: '당신은 아주 극소수의 이들만이 알고 있는 것들을 목격했다.'},
						{ requiredValue: 500,	level: 6, color: 'red',		description: '당신은 타락의 심연을 들여다보았다.'},
						{ requiredValue: 1000,	level: 7, color: 'red',		description: '당신의 성지식은 초월적이다.'}
					]
				}>>

				<!--PHYSIQUE-->
				<<set _physiqueConfig = { name : "체격", icon : "ui/sym_physique", displayType : "level", currentValue : $physique, maxValue: $physiquesize, modifier: 100, modTypes: { good: [], bad: [] },
					states : [
						{ requiredValue:	0,							level: 0, color: 'red',		description: '당신은 수척해져 있다.'},
						{ requiredValue:	$physiquesize / 7,			level: 1, color: 'pink',	description: '당신은 비쩍 말랐다.'},
						{ requiredValue:	($physiquesize / 7) * 2,	level: 2, color: 'purple',	description: '당신의 신체는 유연하고 호리호리하다.'},
						{ requiredValue:	($physiquesize / 7) * 3,	level: 3, color: 'blue',	description: '당신의 신체는 날씬하다.'},
						{ requiredValue:	($physiquesize / 7) * 4,	level: 4, color: 'lblue',	description: '당신의 신체는 날씬하고 탄탄하다.'},
						{ requiredValue:	($physiquesize / 7) * 5,	level: 5, color: 'teal',	description: '당신의 신체는 탄력있고 굳건하다.'},
						{ requiredValue:	($physiquesize / 7) * 6,	level: 6, color: 'green',	description: '당신의 신체는 탄력있고 강하다.'}
					]
				}>>
				<<if playerBellySize() gte 10 and playerNormalPregnancyTotal() lt 50>>
					<<switch playerNormalPregnancyTotal()>>
						<<case 0>><<set _pregnancyModifier to 36>>
						<<case 1>><<set _pregnancyModifier to 48>>
						<<case 2>><<set _pregnancyModifier to 60>>
						<<case 3 4 5>><<set _pregnancyModifier to 78>>
						<<case 6 7>><<set _pregnancyModifier to 96>>
						<<default>><<set _pregnancyModifier to 120>>
					<</switch>>
					<<set _physiqueConfig.modifier to Math.floor(_physiqueConfig.modifier * (1 - (playerBellySize() / _pregnancyModifier)))>>
					<<set _physiqueConfig.modTypes.bad.pushUnique(playerAwareTheyArePregnant() ? "임신" : "커다란 배")>>
				<</if>>
				<<switch $location>>
					<<case "forest" "moor" "farm" "alex_farm">>
						<<if $worn.feet.type.includes("heels")>>
							<<set _physiqueConfig.modifier to Math.floor(_physiqueConfig.modifier * (1 - ($worn.feet.reveal / 5000)))>>
							<<set _physiqueConfig.modTypes.bad.pushUnique("하이힐")>>
						<</if>>
						<<if $worn.feet.type.includes("rugged")>>
							<<set _physiqueConfig.modifier to Math.floor(_physiqueConfig.modifier * (1 + (currentSkillValue("feetskill") / 10000)))>>
							<<set _physiqueConfig.modTypes.good.pushUnique("튼튼한 신발")>>
						<</if>>
				<</switch>>

				<!--WILLPOWER-->
				<<set _willpowerConfig = { name : "의지력", icon : "ui/redbolt", displayType : "level", currentValue : $willpower, maxValue: $willpowermax, modifier: 100, modTypes: { good: [], bad: [] },
					states : [
						{ requiredValue:	0,						level: 0, color: 'red',		description: '당신은 소심하다.'},
						{ requiredValue:	$willpowermax / 7,		level: 1, color: 'pink',	description: '당신은 겁이 많다.'},
						{ requiredValue:	($willpowermax / 7) * 2,	level: 2, color: 'purple',	description: '당신은 조심스럽다.'},
						{ requiredValue:	($willpowermax / 7) * 3,	level: 3, color: 'blue',	description: '당신은 결의를 다지고 있다.'},
						{ requiredValue:	($willpowermax / 7) * 4,	level: 4, color: 'lblue',	description: '당신은 단호하다.'},
						{ requiredValue:	($willpowermax / 7) * 5,	level: 5, color: 'teal',	description: '당신은 완강하다.'},
						{ requiredValue:	($willpowermax / 7) * 6,	level: 6, color: 'green',	description: '당신은 강철의 의지를 갖고 있다.'}
					]
				}>>
				<<if numberOfEarSlime() gte 2 and $earSlime.growth gt 50>>
					<<set _willpowerConfig.modifier to Math.floor(_willpowerConfig.modifier * (0.9 - Math.clamp(($earSlime.growth - 50) / 1000, 0, 0.1)))>>
					<<set _willpowerConfig.modTypes.bad.pushUnique("귀 슬라임")>>
				<<elseif numberOfEarSlime() gte 2>>
					<<set _willpowerConfig.modifier to Math.floor(_willpowerConfig.modifier * 0.9)>>
					<<set _willpowerConfig.modTypes.bad.pushUnique("귀 슬라임")>>
				<</if>>

				<!--BEAUTY-->
				<<set _beautyConfig = { name : "미모", icon : "ui/sym_beauty", displayType : "level", currentValue : $beauty, maxValue: $beautymax, modifier: 100, modTypes: { good: [], bad: [] },
					states : [
						{ requiredValue:	0,						level: 0, color: 'red',		description: '당신은 평범하다.'},
						{ requiredValue:	$beautymax / 7,			level: 1, color: 'pink',	description: '당신은 귀엽다.'},
						{ requiredValue:	($beautymax / 7) * 2,	level: 2, color: 'purple',	description: '당신은 예쁘다.'},
						{ requiredValue:	($beautymax / 7) * 3,	level: 3, color: 'blue',	description: '당신은 매력적이다.'},
						{ requiredValue:	($beautymax / 7) * 4,	level: 4, color: 'lblue',	description: '당신은 아름답다.'},
						{ requiredValue:	($beautymax / 7) * 5,	level: 5, color: 'teal',	description: '당신은 매혹적이다.'},
						{ requiredValue:	($beautymax / 7) * 6,	level: 6, color: 'green',	description: '당신의 미모는 성스러운 수준이다.'}
					]
				}>>

				<!--PROMISCUITY-->
				<<set _promiscuityConfig = { name : "음란", icon : "ui/sym_promiscuity", displayType : "level", currentValue : $promiscuity, maxValue: 100, modifier: 100, modTypes: { good: [], bad: [] },
					states : [
						{ requiredValue: 0,		level: 0, color: 'green',	description: '당신은 순결하고 순수하다.'},
						{ requiredValue: 1,		level: 1, color: 'teal',	description: '당신은 내숭을 떨고 있다.'},
						{ requiredValue: 15,	level: 2, color: 'lblue',	description: '당신은 성적인 호기심이 많다.'},
						{ requiredValue: 35,	level: 3, color: 'blue',	description: '성적 접촉에 대해 생각하면 흥분된다.'},
						{ requiredValue: 55,	level: 4, color: 'purple',	description: '당신은 성적 접촉을 갈구한다.'},
						{ requiredValue: 75,	level: 5, color: 'pink',	description: '당신은 걸레다.'},
						{ requiredValue: 95,	level: 6, color: 'red',		description: '당신의 성욕은 채워지질 않는다.'}
					]
				}>>

				<!--EXHIBITIONISM-->
				<<set _exhibitionismConfig = { name : "노출증", icon : "ui/sym_exhibitionism", displayType : "level", currentValue : $exhibitionism, maxValue: 100, modifier: 100, modTypes: { good: [], bad: [] },
					states : [
						{ requiredValue: 0,		level: 0, color: 'green',	description: '당신은 수줍어한다.'},
						{ requiredValue: 1,		level: 1, color: 'teal',	description: '당신은 부끄러움을 탄다.'},
						{ requiredValue: 15,	level: 2, color: 'lblue',	description: '당신은 성적 대상이 되는 걸 좋아한다.'},
						{ requiredValue: 35,	level: 3, color: 'blue',	description: '당신은 음란한 이목을 끄는 걸 즐긴다.'},
						{ requiredValue: 55,	level: 4, color: 'purple',	description: '노출된다는 느낌에 흥분된다.'},
						{ requiredValue: 75,	level: 5, color: 'pink',	description: '당신은 수치심이 없다.'},
						{ requiredValue: 95,	level: 6, color: 'red',		description: '노출할 생각에 당신의 마음은 완전히 자유분방해진다.'}
					]
				}>>

				<!--DEVIANCY-->
				<<set _deviancyConfig = { name : "이상성욕", icon : "ui/sym_deviancy", displayType : "level", currentValue : $deviancy, maxValue: 100, modifier: 100, modTypes: { good: [], bad: [] },
					states : [
						{ requiredValue: 0,		level: 0, color: 'green',	description: '당신은 비위가 약하다.'},
						{ requiredValue: 1,		level: 1, color: 'teal',	description: '당신은 평범하다.'},
						{ requiredValue: 15,	level: 2, color: 'lblue',	description: '당신의 취향은 이상하다.'},
						{ requiredValue: 35,	level: 3, color: 'blue',	description: '당신의 취향은 충격적이다.'},
						{ requiredValue: 55,	level: 4, color: 'purple',	description: '당신의 욕망은 언어도단 수준이다.'},
						{ requiredValue: 75,	level: 5, color: 'pink',	description: "당신은 다른 이들은 상상도 못할 행위들을 꿈꾼다."},
						{ requiredValue: 95,	level: 6, color: 'red',		description: '당신은 이루 말할 수 없는 것들에 대한 욕망으로 가득하다.'}
					]
				}>>

				<!-- MOOR LUCK, at the end so it appears at the end of the mouseover tooltip -->
				<<if $moorLuck gt 0>>
					<<set _physiqueConfig.modifier to Math.floor(_physiqueConfig.modifier * (1 + ($moorLuck / 100)))>>
					<<set _physiqueConfig.modTypes.good.pushUnique("<i>????</i>")>>
					<<set _willpowerConfig.modifier to Math.floor(_willpowerConfig.modifier * (1 + ($moorLuck / 100)))>>
					<<set _willpowerConfig.modTypes.good.pushUnique("<i>????</i>")>>
				<</if>>
			<</silently>>

			<<characteristic-box _purityConfig>>
			<<characteristic-box _beautyConfig>>
			<<characteristic-box _physiqueConfig>>
			<<characteristic-box _willpowerConfig>>
			<<characteristic-box _awarenessConfig>>
			<<characteristic-box _promiscuityConfig>>
			<<characteristic-box _exhibitionismConfig>>
			<<characteristic-box _deviancyConfig>>
		</div>

		<!--Common states for skills with grades-->
		<<silently>>
			<<set _basicSkillGrades=[
				{ requiredValue: 0,		level: "없음",	color: 'red'},
				{ requiredValue: 1,		level: "F",		color: 'pink'},
				{ requiredValue: 200,	level: "D",		color: 'purple'},
				{ requiredValue: 400,	level: "C",		color: 'blue'},
				{ requiredValue: 600,	level: "B",		color: 'lblue'},
				{ requiredValue: 800,	level: "A",		color: 'teal'},
				{ requiredValue: 1000,	level: "S",		color: 'green'}
			]>>
			<<set _detailedSkillGrades=[
				{ requiredValue: 0,		level: "없음",	color: 'red'},
				{ requiredValue: 1,		level: "F",		color: 'pink'},
				{ requiredValue: 100,	level: "F+",	color: 'pink'},
				{ requiredValue: 200,	level: "D",		color: 'purple'},
				{ requiredValue: 300,	level: "D+",	color: 'purple'},
				{ requiredValue: 400,	level: "C",		color: 'blue'},
				{ requiredValue: 500,	level: "C+",	color: 'blue'},
				{ requiredValue: 600,	level: "B",		color: 'lblue'},
				{ requiredValue: 700,	level: "B+",	color: 'lblue'},
				{ requiredValue: 800,	level: "A",		color: 'teal'},
				{ requiredValue: 900,	level: "A+",	color: 'teal'},
				{ requiredValue: 1000,	level: "S",		color: 'green'}
			]>>
		<</silently>>

		<br>
		<span class="gold">일반 기술</span>
		<div id="base-skills" class="characteristic-box-list" @style="($parasite.left_ear.name == 'slime' || $parasite.right_ear.name == 'slime') ? '--min-width: 30%;' : ''">
			<!--Additional skills configurations-->
			<<silently>>
				<<set _corruptionConfig =	{ name : "오염",	displayType : "none",	currentValue : $earSlime.corruption,	modifier: 100, modTypes: { good: [], bad: [] },		meterColor: $earSlime.startedThreats ? "purple" : "blue", secondValue: $earSlime.growth / 2, secondMeterColor: "red"}>>
				<<set _skulduggeryConfig =	{ name : "속임수", displayType : "grade",	currentValue : $skulduggery,		modifier: 100, modTypes: { good: [], bad: [] },		states : _detailedSkillGrades}>>
				<<set _dancingConfig =		{ name : "춤",		displayType : "grade",	currentValue : $danceskill,			modifier: 100, modTypes: { good: [], bad: [] },		states : _detailedSkillGrades}>>
				<<set _swimmingConfig =		{ name : "수영",	displayType : "grade",	currentValue : $swimmingskill,		modifier: 100, modTypes: { good: [], bad: [] },		states : _detailedSkillGrades}>>
				<<set _athleticsConfig =	{ name : "운동",	displayType : "grade",	currentValue : $athletics,			modifier: 100, modTypes: { good: [], bad: [] },		states : _detailedSkillGrades}>>
				<<set _tendingConfig =		{ name : "관리",		displayType : "grade",	currentValue : $tending,			modifier: 100, modTypes: { good: [], bad: [] },		states : _detailedSkillGrades}>>
				<<set _housekeepingConfig =	{ name : "정리정돈",displayType : "grade",	currentValue : $housekeeping,		modifier: 100, modTypes: { good: [], bad: [] },		states : _detailedSkillGrades}>>
				<<set _bellySize to playerBellySize()>>
				<<if _bellySize gte 10 and playerNormalPregnancyTotal() lt 50>>
					<<switch playerNormalPregnancyTotal()>>
						<<case 0>><<set _pregnancyModifier to 36>>
						<<case 1>><<set _pregnancyModifier to 48>>
						<<case 2>><<set _pregnancyModifier to 60>>
						<<case 3 4 5>><<set _pregnancyModifier to 78>>
						<<case 6 7>><<set _pregnancyModifier to 96>>
						<<default>><<set _pregnancyModifier to 120>>
					<</switch>>
					<<set _dancingConfig.modifier to Math.floor(_dancingConfig.modifier * (1 - (_bellySize / _pregnancyModifier)))>>
					<<set _dancingConfig.modTypes.bad.pushUnique(playerAwareTheyArePregnant() ? "임신" : "커다란 배")>>
					<<set _swimmingConfig.modifier to Math.floor(_swimmingConfig.modifier * (1 - (_bellySize / _pregnancyModifier)))>>
					<<set _swimmingConfig.modTypes.bad.pushUnique(playerAwareTheyArePregnant() ? "임신" : "커다란 배")>>
					<<set _athleticsConfig.modifier to Math.floor(_athleticsConfig.modifier * (1 - (_bellySize / _pregnancyModifier)))>>
					<<set _athleticsConfig.modTypes.bad.pushUnique(playerAwareTheyArePregnant() ? "임신" : "커다란 배")>>
				<</if>>
				<<if $worn.hands.type.includes("sticky_fingers")>>
					<<set _skulduggeryConfig.modifier to Math.floor(_skulduggeryConfig.modifier * 1.05)>>
					<<set _skulduggeryConfig.modTypes.good.pushUnique("끈적한 손가락")>>
				<</if>>
				<<if $transformationParts.traits.sharpEyes isnot "disabled">>
					<<set _skulduggeryConfig.modifier to Math.floor(_skulduggeryConfig.modifier * 1.05)>>
					<<set _skulduggeryConfig.modTypes.good.pushUnique("날카로운 눈")>>
				<</if>>
				<<if $fox gte 6>>
					<<set _skulduggeryConfig.modifier to Math.floor(_skulduggeryConfig.modifier * 1.10)>>
					<<set _skulduggeryConfig.modTypes.good.pushUnique("여우")>>
				<</if>>
				<<if $worn.under_upper.type.includesAny("dance", "naked") and $worn.under_lower.type.includesAny("dance", "naked")
				and $worn.upper.type.includesAny("dance", "naked")and $worn.lower.type.includesAny("dance", "naked")>>
					<<set _dancingConfig.modifier to Math.floor(_dancingConfig.modifier * 1.05)>>
					<<if [$worn.under_upper.type, $worn.under_lower.type, $worn.upper.type, $worn.lower.type].flat().includes("dance")>>
						<<set _dancingConfig.modTypes.good.pushUnique("무용복")>>
					<<else>>
						<<set _dancingConfig.modTypes.good.pushUnique("알몸")>>
					<</if>>
				<</if>>
				<<if $worn.under_upper.type.includesAny("swim", "naked") and $worn.under_lower.type.includesAny("swim", "naked")
				and $worn.upper.type.includesAny("swim", "naked") and $worn.lower.type.includesAny("swim", "naked")>>
					<<set _swimmingConfig.modifier to Math.floor(_swimmingConfig.modifier * 1.05)>>
					<<if $worn.under_upper.type.includes("swim") or $worn.under_lower.type.includes("swim")
					or $worn.upper.type.includes("swim") or $worn.lower.type.includes("swim")>>
						<<set _swimmingConfig.modTypes.good.pushUnique("수영복")>>
					<<else>>
						<<set _swimmingConfig.modTypes.good.pushUnique("알몸")>>
					<</if>>
				<</if>>
				<<if $worn.feet.type.includes("swim")>>
					<<set _swimmingConfig.modifier to Math.floor(_swimmingConfig.modifier * (1 + (currentSkillValue("feetskill") / 10000)))>>
					<<set _swimmingConfig.modTypes.good.pushUnique("물갈퀴")>>
				<<elseif !$worn.feet.type.includes("naked")>>
					<<if $worn.feet.type.includes("heels")>>
						<<set _heels to 0.1>>

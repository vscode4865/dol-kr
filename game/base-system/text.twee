<<widget "generatePronouns">>
<<if $args[0]>>
	<<set $args[0].pronouns to {}>>
	<<switch $args[0].pronoun>>
	<<case "m">>
		<<set $args[0].pronouns.he = "그는">>
		<<set $args[0].pronouns.his = "그의">>
		<<set $args[0].pronouns.hers = "그의 것">>
		<<set $args[0].pronouns.him = "그를">>
		<<set $args[0].pronouns.himself = "그 자신">>
		<<set $args[0].pronouns.man = "남자">>
		<<set $args[0].pronouns.boy = "소년">>
		<<set $args[0].pronouns.men = "남자들">>
	<<case "f">>
		<<set $args[0].pronouns.he = "그녀는">>
		<<set $args[0].pronouns.his = "그녀의">>
		<<set $args[0].pronouns.hers = "그녀의 것">>
		<<set $args[0].pronouns.him = "그녀를">>
		<<set $args[0].pronouns.himself = "그녀 자신">>
		<<set $args[0].pronouns.man = "여자">>
		<<set $args[0].pronouns.boy = "소녀">>
		<<set $args[0].pronouns.men = "여자들">>
	<<case "i">>
		<<set $args[0].pronouns.he = "녀석은">>
		<<set $args[0].pronouns.his = "녀석의">>
		<<set $args[0].pronouns.hers = "녀석의 것">>
		<<set $args[0].pronouns.him = "녀석을">>
		<<set $args[0].pronouns.himself = "녀석 자신">>
		<<set $args[0].pronouns.man = "어른">>
		<<set $args[0].pronouns.boy = "새끼">>
		<<set $args[0].pronouns.men = "어른들">>
	<<case "n">>
		<<set $args[0].pronouns.he = "그자는">>
		<<set $args[0].pronouns.his = "그자의">>
		<<set $args[0].pronouns.hers = "그자의 것">>
		<<set $args[0].pronouns.him = "그자를">>
		<<set $args[0].pronouns.himself = "그자 자신">>
		<<set $args[0].pronouns.man = "어른">>
		<<set $args[0].pronouns.boy = "아이">>
		<<set $args[0].pronouns.men = "어른들">>
	<<case "t">>
		<<set $args[0].pronouns.he = "그들이">>
		<<set $args[0].pronouns.his = "그들의">>
		<<set $args[0].pronouns.hers = "그들의 것">>
		<<set $args[0].pronouns.him = "그들을">>
		<<set $args[0].pronouns.himself = "그들 자신">>
		<<set $args[0].pronouns.man = "어른">>
		<<set $args[0].pronouns.boy = "아이">>
		<<set $args[0].pronouns.men = "어른들">>
	<</switch>>
<</if>>
<</widget>>


<<widget "generatePronouns">>
	<<if _args[0]>>
		<<set _args[0].pronouns to {}>>
		<<switch _args[0].pronoun>>
			<<case "m">>
				<<set _args[0].pronouns.he = "그는">>
				<<set _args[0].pronouns.his = "그의">>
				<<set _args[0].pronouns.hers = "그의 것">>
				<<set _args[0].pronouns.him = "그를">>
				<<set _args[0].pronouns.himself = "그 자신">>
				<<set _args[0].pronouns.man = "남자">>
				<<set _args[0].pronouns.boy = "소년">>
				<<set _args[0].pronouns.men = "남자들">>
			<<case "f">>
				<<set _args[0].pronouns.he = "그녀는">>
				<<set _args[0].pronouns.his = "그녀의">>
				<<set _args[0].pronouns.hers = "그녀의 것">>
				<<set _args[0].pronouns.him = "그녀를">>
				<<set _args[0].pronouns.himself = "그녀 자신">>
				<<set _args[0].pronouns.man = "여자">>
				<<set _args[0].pronouns.boy = "소녀">>
				<<set _args[0].pronouns.men = "여자들">>
			<<case "i">>
				<<set _args[0].pronouns.he = "녀석은">>
				<<set _args[0].pronouns.his = "녀석의">>
				<<set _args[0].pronouns.hers = "녀석의 것">>
				<<set _args[0].pronouns.him = "녀석을">>
				<<set _args[0].pronouns.himself = "녀석 자신">>
				<<set _args[0].pronouns.man = "어른">>
				<<set _args[0].pronouns.boy = "소년">>
				<<set _args[0].pronouns.men = "어른들">>
			<<case "n">>
				<<set _args[0].pronouns.he = "그자는">>
				<<set _args[0].pronouns.his = "그자의">>
				<<set _args[0].pronouns.hers = "그자의 것">>
				<<set _args[0].pronouns.him = "그자를">>
				<<set _args[0].pronouns.himself = "그자 자신">>
				<<set _args[0].pronouns.man = "어른">>
				<<set _args[0].pronouns.boy = "아이">>
				<<set _args[0].pronouns.men = "어른들">>
			<<case "t">>
				<<set _args[0].pronouns.he = "그들이">>
				<<set _args[0].pronouns.his = "그들의">>
				<<set _args[0].pronouns.hers = "그들의 것">>
				<<set _args[0].pronouns.him = "그들을">>
				<<set _args[0].pronouns.himself = "그들 자신">>
				<<set _args[0].pronouns.man = "어른">>
				<<set _args[0].pronouns.boy = "소년">>
				<<set _args[0].pronouns.men = "어른들">>
		<</switch>>
	<</if>>
<</widget>>

<<widget "clearPronouns">>
	<<if _args[0]>>
		<<set _args[0].pronouns.he = 0>>
		<<set _args[0].pronouns.his = 0>>
		<<set _args[0].pronouns.hers = 0>>
		<<set _args[0].pronouns.him = 0>>
		<<set _args[0].pronouns.himself = 0>>
		<<set _args[0].pronouns.man = 0>>
		<<set _args[0].pronouns.boy = 0>>
		<<set _args[0].pronouns.men = 0>>
	<</if>>
<</widget>>

<!-- [0] - string
	 prints string [0] prepended with an indefinite article based on the [0]'s first character -->
<<widget "a">><<silently>>
	<<if ["a","e","i","o","u","A","E","I","O","U"].includes(_args[0][0])>>
		<<set _text_output to "an " + _args[0]>>
	<<else>>
		<<set _text_output to "a " + _args[0]>>
	<</if>>
<</silently>>_text_output<</widget>>

<<widget "A">><<silently>>
	<<a _args[0]>><<capitalise>>
<</silently>>_text_output<</widget>>

<<widget "theowner">><<silently>>
	<<if $enemyno gte 2 and $enemytype is "man">>
		<<set _text_output to "the owner">>
	<<else>>
		<<he>>
	<</if>>
<</silently>>_text_output<</widget>>

<<widget "someones">>
	<<if _args[0] is "two">>
		their
	<<elseif _args[0] isnot undefined>>
		<<set $_target to _args[0]>>
		<<hisselect $_target>>
	<<else>>
		<<his>>
	<</if>>
<</widget>>

<<widget "someone">><<silently>>

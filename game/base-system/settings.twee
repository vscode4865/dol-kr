<</widget>>

<<widget "npcSettings">>
	<div class="solidBorderContainer settings-container">
		<div class="settingsToggleIndent">
			<span class="gold">네임드 NPC들을 임의로 설정:</span> <mouse class="tooltip linkBlue">(?)<span>일반 NPC 설정을 기반으로 설정될 것입니다.</span></mouse>
			<br>
			<<link "성별">>
				<<initnpcgender "genders">>
				<<updatePersistentNPCs "genders">>
				<<replace #npcSettingsMenu>><<npcSettingsMenu>><</replace>>
			<</link>>
			|
			<<link "자지 크기">>
				<<initnpcgender "penis">>
				<<updatePersistentNPCs "penis">>
				<<replace #npcSettingsMenu>><<npcSettingsMenu>><</replace>>
			<</link>>
			|
			<<link "가슴 크기">>
				<<initnpcgender "breasts">>
				<<updatePersistentNPCs "breasts">>
				<<replace #npcSettingsMenu>><<npcSettingsMenu>><</replace>>
			<</link>>
			|
			<<link "피부색">>
				<<initnpcskin "skincolour">>
				<<updatePersistentNPCs "skincolour">>
				<<replace #npcSettingsMenu>><<npcSettingsMenu>><</replace>>
			<</link>>
			<<if $debug is 1>>
				|
				<<link "머리카락과 눈 색 초기화 (기본 색상을 가진 NPC에게만 영향을 줍니다)">>
					<<generateNPCNameHairAndEyeColors "default">>
					<<displaySettings>>
				<</link>>
			<</if>>
		</div>
		<hr>
		<<if _npcId is undefined>>
			<<set _npcId to 0>>

			<<set _breastDescription to ["Flat","Small","Pert","Modest","Full","Large"]>>
			<<set _breastDescriptionNPC to ["nipple","small breast","pert breast","modest breast","full breast","large breast"]>>
			<<set _breastsDescriptionNPC to ["nipples","small breast","pert breast","modest breast","full breast","large breast"]>>

			<<set _hairColorNPC to ["Red","Black","Brown","Light Brown","Blond","Platinum blond","Strawberry blond","Ginger", "Mousy"]>>
			<<set _hairColorNPCData to ["red","black","brown","lightbrown","blond","platinumblond","strawberryblond","ginger", "mousy"]>>
			<<set _hairColorNPCText to ["red","black","brown","lightbrown","gold","platinum","strawberry","tangerine", "mousy"]>>

			<<set _eyeColorNPC to ["Purple","Dark Blue","Light Blue","Amber","Hazel","Green","Lime Green","Red","Pink","Grey","Light Grey"]>>
			<<set _eyeColorNPCData to ["purple","dark blue","light blue","amber","hazel","green", "lime green","red","pink","grey", "light grey"]>>
			<<set _eyeColorNPCText to ["purple","blue","lblue","tangerine","brown","green","lime-green","red","pink","grey", "light-grey"]>>
		<</if>>

		<<if _npcList is undefined>>
			<<set _npcList to {}>>
			<<for $_i to 0; $_i lt $NPCNameList.length; $_i++>>
				<<set $_name to $NPCNameList[$_i]>><<trNamedNPC $_name "name">><<set _npcList[_trResult] to clone($_i)>>
			<</for>>
			<<if $passage is "StartScene" or !$wraithIntro>>
				<<run delete _npcList["상아 유령"]>>
			<</if>>
		<</if>>

		/* Updates the npc's in case $incompletePregnancyDisable has been changed by the player */
		<<npcPregnancyUpdater>>
		<div id="npcSettingsMenu">
			<<npcSettingsMenu>>
		</div>


		/*이건 StartScene로 수정 해야할지 안해야할지 모르겠음. 좀 더 알아보고 결정함 */
		<<if $passage isnot "start" and $per_npc and Object.keys($per_npc).length gt 0>>
			<<run
				if (!T.perNPCLock) {
					$(".passage").on('change', '#perNPCSettingsMenu .macro-listbox', function (e) {
						new Wikifier(null, '<<replace #perNPCSettingsMenu>><<perNPCSettingsMenu>><</replace>>');
					});

					T.keys = {};
					T.keyNames = [];
					const allowed_npcs = ["steed", "guard", "inmate", "panty_thief", "school_nurse", "tower_creature", "pinch", "black_dog", "hacker", "bishop", "bishop_right_hand", "bishop_left_hand"];
					Object.keys(V.per_npc).forEach(k => {
						for (const j of allowed_npcs){
							if (k.includes(j)){
								T.keyNames.push(k);
								T.keys[k.toUpperFirst().replace(/_/g, ' ')] = k;
								break
							}
						}
					});
					T.perNPCLock = true;
				}
			>>
			<<if T.keyNames and T.keyNames.length>>
				<hr>
				<div id="perNPCSettingsMenu">
					<<perNPCSettingsMenu>>
				</div>
			<</if>>
		<</if>>

		<<if _listActive isnot true>>
			<<NPCSettingsReset>>
			<<set _listActive to true>>
		<</if>>
	</div>
<</widget>>

<<widget "exportsettings">>
	<div class="solidBorderContainer settings-container">
	<div class="settings-intro">
	<span class="yellow">"저장/옵션" 오버레이의 설정도 불러오기/내보내기 하게 됩니다.</span>
	<hr>
	<h4 class="gold">설정 파일 불러오기/내보내기</h4>

	<<if SugarCube.Browser.isMobile.any() is true>>
		파일로부터 저장 및 불러오는 기능은 안드로이드 버전에서는 현재 작동하지 않습니다.
	<<else>>
		파일에서 불러오기: <input id="saveImport" type="file" value="파일에서 불러오기" onchange="importSettings(this.files, 'file')">
		<br><br>
		<<if $ironmanmode is true>>
			<input type="button" value="파일로 저장" disabled>
		<<else>>
			<input type="button" value="파일로 저장" onclick="exportSettings(null,'file')">
		<</if>>
	<</if>>
	<hr>
	<h4 class="gold">설정 텍스트 불러오기/내보내기</h4>

	<ul>
	<li>현재 설정을 저장하려면, "설정 데이터 가져오기"를 누른 뒤, 세이브 데이터를 복사하여 요구된 곳에 붙여넣기 하세요.</li>
	<li>현재 설정을 불러오려면, 설정 데이터를 붙여넣은 뒤 "설정 데이터 불러오기"를 누르세요.</li>
	<li>안드로이드로부터 설정을 주고 받으려면, 이메일을 사용하거나 pastebin 같은 서비스를 이용하시는 걸 추천합니다. 문제가 생기는 것을 방지하기 위해 세이브의 시작과 끝이 동일한지 꼼꼼하게 점검하세요.</li>
	<li>"텍스트 영역 복사"를 누르면 내용물을 클립보드에 복사합니다. 장치나 브라우저가 지원하지 않는다면 버튼의 이름이 "복사 에러"로 변할 겁니다.</li>
	</ul>
	<textarea id="settingsDataInput" rows="4" cols="40" onClick="if(V.disableAutoSelect == false)this.select();" onfocus="V.tempDisable = true;" onblur="V.tempDisable = false;"></textarea>
	<br>
	<input type="button" value="텍스트 상자 비우기" onclick="clearTextBox('settingsDataInput')"/>
	<input type="button" value="텍스트 상자 최상단" onclick="topTextArea('settingsDataInput')"/>
	<input type="button" value="텍스트 상자 최하단" onclick="bottomTextArea('settingsDataInput')"/>
	<input type="button" value="설정 데이터 가져오기" onclick="exportSettings(null,'text')"/>
	<input type="button" value="설정 데이터 불러오기" onclick="importSettings(null,'text')"/>
	<input id="CopyTextArea" type="button" value="텍스트 영역 복사" onclick="copySavedata('settingsDataInput')">
	<input id="LoadExternalExportFile" type="button" value="외부 파일 불러오기" onclick="loadExternalExportFile()">

	<br><br>

	<label>
		<<checkbox "$disableAutoSelect" false true autocheck>>
		자동 선택 비활성화.
	</label>
	</div>
	</div>
<</widget>>

<<widget "startDescriptions">>
	<div class="solidBorderContainer settings-container">
		<div class="settings-intro">
			<<switch _args[0]>>
				<<case "default">><span class="gold bold">기본 시작:</span> 페티쉬 설정과 난이도 설정은  <span class="teal">기본값</span>으로 설정됩니다. NPC들은 <span class="teal">무작위</span>로 생성됩니다. <i class="teal">초심자에게 추천합니다.</i>
				<<case "adjusted">><span class="gold bold">중급 시작:</span> 당신은 난이도, 페티쉬, 그리고 NPC 성별을 <span class="teal">개괄적으로 변경</span>할 수 있습니다. NPC들은 이 설정에 따라 <span class="teal">무작위로</span> 생성됩니다.
				<<case "custom">><span class="gold bold">고급 시작:</span> 당신은 당신의 욕구에 맞춰서, 페티쉬, 난이도, 그리고 NPC 성별을 포함한 <span class="teal">모든 설정</span>을 조절할 수 있습니다.
				<<case "random">><span class="gold bold">무작위 시작:</span> 캐릭터 모습, 캐릭터 특성, 그리고 난이도 설정을 포함한 <span class="teal">모든 설정</span>을 무작위로 설정하고 게임을 시작합니다. 완벽하게 예측할 수 없는 게임 진행을 위하여 당신의 캐릭터와 NPC들이 모두 <span class="teal">무작위</span>로 생성됩니다. <i><span class="teal">게임을 시작하기 전에 특정 설정만을 무작위화하기 위해서는, 이것 대신</span> <span class="gold">고급 시작</span><span class="teal">을 선택하세요.</span></i>
			<</switch>>
			<br>
			<div class="small-description">대부분의 설정은 당신의 방에서 나중에 변경이 가능합니다. <span class="red">캐릭터의 성기나 도전과제 부스트는 게임 시작 이후에 변경할 수 없습니다.</span></div>
		</div>
	</div>
<</widget>>

<<widget "npcSettingsMenu">>
	<div class="settingsToggleIndent">
		<span class="gold">특정한 NPC 설정: </span>
		<<listbox "_npcId" autoselect>>
			<<optionsfrom _npcList>>
		<</listbox>>
	</div>
	<div class="settingsGrid">
		<div class="settingsHeader">
			<span style="text-transform: capitalize;"><<trNamedNPC $NPCName[_npcId].title>>_trResult</span> <<trNamedNPC $NPCName[_npcId].nam>>_trResult
			<span style="font-size:80%">
				<br>
				<<link "이전">>
					<<if _npcId is 0>>
						<<set _npcId to $NPCNameList.length - 1>>
					<<else>>
						<<set _npcId-->>
					<</if>>
					<<trNamedNPC $NPCName[_npcId].nam "name">><<if _npcList[_trResult] is undefined>>
						<<set _npcId-->>
					<</if>>
					<<replace #npcSettingsMenu>><<npcSettingsMenu>><</replace>>
				<</link>> <span style="color:white">|</span>
				<<link "다음">>
					<<if _npcId is $NPCNameList.length - 1>>
						<<set _npcId to 0>>
					<<else>>
						<<set _npcId++>>
					<</if>>
					<<trNamedNPC $NPCName[_npcId].nam "name">><<if _npcList[_trResult] is undefined>>
						<<set _npcId++>>
					<</if>>
					<<replace #npcSettingsMenu>><<npcSettingsMenu>><</replace>>
				<</link>>
			</span>
		</div>
		<<if V.NPCName[T.npcId].pregnancy.type>>
			<div class="settingsToggleItem">
				<br><br>
				이 NPC는 현재 임신중입니다. 임신중에는 몇몇 설정은 변경할 수 없습니다.
			</div>
		<</if>>
		<div class="settingsToggleItem">
			<span class="gold">성별:</span>
			<br>
			<span data-target="npcname-npcidpronoun" data-disabledif="V.NPCName[T.npcId].pronoun==='i'">
			<label><<radiobutton "$NPCName[_npcId].pronoun" "f" autocheck>> 여성</label> |
			<label><<radiobutton "$NPCName[_npcId].pronoun" "m" autocheck>> 남성</label>
			</span>
		</div>
		<div class="settingsToggleItem">
			<span class="gold">성기:</span>
			<br>
			<span data-target="npcname-pregnancy" data-disabledif="V.NPCName[T.npcId].pregnancy.type">
				<label><<radiobutton "$NPCName[_npcId].gender" "f" autocheck>> 보지</label> |
				<label><<radiobutton "$NPCName[_npcId].gender" "m" autocheck>> 자지</label>
				<<if $debug is 1>>| <label><<radiobutton "$NPCName[_npcId].gender" "h" autocheck>> 둘 다</label><</if>>
			</span>
		</div>
		<div class="settingsToggleItem">
			<span class="gold">자지 크기 - 가능할 경우:</span>
			<br>
			<<set _penisNames to ["", "조그마함", "평균", "두꺼움", "커다람"]>> /* the empty first element is necessary because _j starts at 1 */
			<span data-target="npcname-npcidgender" data-disabledif="V.NPCName[T.npcId].gender==='f'">
				<<for _j to 1; _j lt _penisNames.length; _j++>>
					<label>
						<<radiobutton "$NPCName[_npcId].penissize" _j autocheck>>
						<<print _penisNames[_j]>>
					</label>
					<<if _j isnot _penisNames.length -1>>|<</if>>
				<</for>>
			</span>
		</div>
		<div class="settingsToggleItem">
			<span data-target="npcname-pregnancy" data-disabledif="setup.pregnancy.infertile.includes(V.NPCName[T.npcId].nam)||V.NPCName[T.npcId].pregnancy.enabled===undefined||V.NPCName[T.npcId].pregnancy.type||!setup.pregnancy.typesEnabled.includes(V.NPCName[T.npcId].type)">
				<span class="gold">NPC 임신 활성화 - 가능할 경우:</span>
				<mouse class="tooltip linkBlue">(?)<span>'NPC의 임신 활성화' 설정이 활성화되어 있어야 합니다. 또한 "미완성된 임신 컨텐츠 활성화" 설정이 활성화되어 있어야 할 수도 있습니다. 이미 존재하는 임신을 멈추게 하지는 않습니다.</span></mouse>
				<br>
				<label><<radiobutton "$NPCName[_npcId].pregnancy.enabled" true autocheck>> 가능</label> |
				<label><<radiobutton "$NPCName[_npcId].pregnancy.enabled" false autocheck>> 불가능</label>
			</span>
		</div>

		<div class="settingsToggleItem">
		<span class="gold">가슴 크기:</span>
		<br>
		<<numberslider "$NPCName[_npcId].breastsize" $NPCName[_npcId].breastsize 0 12 1>>
		<<run settingsNamedNpcBreastSize(Util.slugify('$NPCName[_npcId].breastsize'), false)>>
		</div>

		<div class="settingsToggleItem">
			<span class="gold">피부색:</span>
			<br>
			<<if $NPCName[_npcId].nam is "Black Wolf">>/*disable skin colour toggle when player has BW set to 'always a beast'*/
				<span data-target="blackwolfmonster" data-disabledif="V.blackwolfmonster===0">
				<label><<radiobutton "$NPCName[_npcId].skincolour" "white" autocheck>> 옅음</label> |
				<label><<radiobutton "$NPCName[_npcId].skincolour" "black" autocheck>> 짙음</label>
				</span>
			<<elseif $NPCName[_npcId].nam is "Great Hawk">>/*disable skin colour toggle when player has BW set to 'always a beast'*/
				<span data-target="greathawkmonster" data-disabledif="V.greathawkmonster===0">
				<label><<radiobutton "$NPCName[_npcId].skincolour" "white" autocheck>> 옅음</label> |
				<label><<radiobutton "$NPCName[_npcId].skincolour" "black" autocheck>> 짙음</label>
				</span>
			<<else>>
				<span data-target="npcname-npcidskincolour" data-disabledif="V.NPCName[T.npcId].skincolour==='ghost'">
				<label><<radiobutton "$NPCName[_npcId].skincolour" "white" autocheck>> 옅음</label> |
				<label><<radiobutton "$NPCName[_npcId].skincolour" "black" autocheck>> 짙음</label>
				</span>
			<</if>>
		</div>

		<<if $NPCName[_npcId].nam is "Black Wolf">>
			<div class="settingsToggleItemWide">
				<span class="gold">짐승 종류:</span>
				<br>
				<label><<radiobutton "$blackwolfmonster" 0 autocheck>> 항상 짐승</label> |
				<label><<radiobutton "$blackwolfmonster" 1 autocheck>> 조건이 맞는다면 몬스터 소녀나 소년</label> |
				<label><<radiobutton "$blackwolfmonster" 2 autocheck>> 항상 몬스터 소녀나 소년</label>
			</div>
		<<elseif $NPCName[_npcId].nam is "Great Hawk">>
			<div class="settingsToggleItemWide">
				<span class="gold">짐승 종류:</span>
				<br>
				<label><<radiobutton "$greathawkmonster" 0 autocheck>> 항상 짐승</label> |
				<label><<radiobutton "$greathawkmonster" 1 autocheck>> 조건이 맞는다면 몬스터 소녀나 소년</label> |
				<label><<radiobutton "$greathawkmonster" 2 autocheck>> 항상 몬스터 소녀나 소년</label>
			</div>
		<</if>>
	</div>
	<div style="clear:both;"></div>

	<<if $debug is 1>>
		DEBUG:
		<br>
		<div class="settingsToggleItemWide">
			<span class="gold">머리색:</span>
			<br>
			<<for _j to 0; _j lt _hairColorNPC.length; _j++>>
				<label>
					<<print '<<radiobutton "$NPCName['+_npcId+'].hairColour" "'+_hairColorNPCData[_j]+'" ' + ($NPCName[_npcId].hairColour is _hairColorNPCData[_j] ? "checked" : "") + '>>'>>
					<span @class="_hairColorNPCText[_j]"><<trColour _hairColorNPC[_j]>>_trResult</span>
				</label> |
			<</for>>
		</div>

		<div class="settingsToggleItemWide">
			<span class="gold">눈 색:</span>
			<br>
			<<for _j to 0; _j lt _eyeColorNPC.length; _j++>>
				<label>
					<<print '<<radiobutton "$NPCName['+_npcId+'].eyeColour" "'+_eyeColorNPCData[_j]+'" ' + ($NPCName[_npcId].eyeColour is _eyeColorNPCData[_j] ? "checked" : "") + '>>'>>
					<span @class="_eyeColorNPCText[_j]"><<trColour _eyeColorNPC[_j]>>_trResult</span>
				</label> |
			<</for>>
		</div>
		<div style="clear:both;"></div>
	<</if>>
	<<run settingsDisableElement()>>
<</widget>>

<<widget "perNPCSettingsMenu">>
	<span class="gold">지속성 NPC 설정 (Persistent; 더 나은 번역이 있으면 수정할 것):</span>
	<br>
	<<listbox "_pNPCId" autoselect>>
		<<optionsfrom _keys>>
	<</listbox>>
	<<switch _pNPCId>>
		<<case "scarred_inmate">>
			<<if $per_npc[_pNPCId].description isnot "scarred">>
				<<generate_scarred_inmate 0>>
				<<clearsinglenpc 0>>
			<</if>>
		<<case "veteran_guard">>
			<<if $per_npc[_pNPCId].description isnot "veteran">>
				<<generate_veteran_guard 0>>
				<<clearsinglenpc 0>>
			<</if>>
	<</switch>>
	<<set _index to _keyNames.indexOf(_pNPCId)>>
	| <<link "이전">>
		<<set _index to (_index is 0 ? _keyNames.length - 1 : --_index)>>
		<<set _pNPCId to _keyNames[_index]>>
		<<replace #perNPCSettingsMenu>><<perNPCSettingsMenu>><</replace>>
	<</link>> |
	<<link "다음">>
		<<set _index++>>
		<<set _pNPCId to _keyNames[_index]>>
		<<replace #perNPCSettingsMenu>><<perNPCSettingsMenu>><</replace>>
	<</link>>
	<br><br>
	<<if $per_npc[_pNPCId] eq null>>
		<<error {
			message	: `The given ID for pNPCs failed to match any available pNPC: ${_pNPCId}.`,
			source	: _pNPCId
		}>>
	<<else>>
		<<if $per_npc[_pNPCId].name_known>>
			<span class="gold"><<print _pNPCId.replace(/_/g, ' ')>> <<trNPCname $per_npc[_pNPCId].name>>_trResult</span>
		<<elseif _pNPCId is "tower_creature">>
			<span class="gold">밤의 괴물</span>
		<<elseif _pNPCId is "hacker">>
			<span class="gold">해커 '미키'</span>
		<<elseif _pNPCId is "bishop">>
			<span class="gold">주교</span>
		<<elseif _pNPCId is "bishop_right_hand">>
			<span class="gold">주교의 오른손</span>
		<<elseif _pNPCId is "bishop_left_hand">>
			<span class="gold">주교의 왼손</span>
		<<else>>
			<span class="gold"><<print _pNPCId.toUpperFirst().replace(/_/g, ' ')>></span>
		<</if>>
		<<if $per_npc[_pNPCId].name_known and $debug is 1>>
			| <<link "이름 변경">>
				<<if $per_npc[_pNPCId].pronoun is "f">>
					<<set $per_npc[_pNPCId].name to $NPC_names_f.random()>>
				<<else>>
					<<set $per_npc[_pNPCId].name to $NPC_names_m.random()>>
				<</if>>
				<<replace #perNPCSettingsMenu>><<perNPCSettingsMenu>><</replace>>
			<</link>>
		<</if>>
		<br>
		<div class="settingsToggle">
			<span data-target="pernpc-npcidpronoun" data-disabledif="V.per_npc[T.pNPCId].pronoun==='i'||V.per_npc[T.pNPCId].pronoun==='t'">
			<label><<radiobutton "$per_npc[_pNPCId].pronoun" "f" autocheck>> 여성</label> |
			<label><<radiobutton "$per_npc[_pNPCId].pronoun" "m" autocheck>> 남성</label>
			</span>
		</div>
		<div class="settingsToggle">
			<label><<radiobutton "$per_npc[_pNPCId].gender" "f" autocheck>> 보지</label> |
			<label><<radiobutton "$per_npc[_pNPCId].gender" "m" autocheck>> 자지</label>
		</div>
		<div class="settingsToggle">
			자지 크기 - 가능할 경우:
			<br>
			<<set _penisNames to ["", "조그마함", "평균", "두꺼움", "커다람"]>> /* the empty first element is necessary because _j starts at 1 */
			<span data-target="pernpc-npcidgender" data-disabledif="V.per_npc[T.pNPCId].gender==='f'">
			<<for _j to 1; _j lt _penisNames.length; _j++>>
				<label>
					<<radiobutton "$per_npc[_pNPCId].penissize" _j autocheck>>
					<<print _penisNames[_j]>>
				</label>
				<<if _j isnot _penisNames.length -1>>|<</if>>
			<</for>>
			</span>
		</div>
		<div class="settingsToggle" style="display: inline-flex;">
			가슴 크기:
			<<numberslider "$per_npc[_pNPCId].breastsize" $per_npc[_pNPCId].breastsize 0 12 1>>
			<<run settingsNamedNpcBreastSize(Util.slugify('$per_npc[_pNPCId].breastsize'), true)>>
		</div>
		<div class="settingsToggle">
			피부색:
			<br>
			<label><<radiobutton "$per_npc[_pNPCId].skincolour" "white" autocheck>> 옅음</label> |
			<label><<radiobutton "$per_npc[_pNPCId].skincolour" "black" autocheck>> 짙음</label>
		</div>
	<</if>>
	<div style="clear:both;"></div>
	<<run settingsDisableElement()>>
<</widget>>

<<widget "saveNameSection">>
	<span class="gold bold">세이브 이름:</span> <i>(optional)</i>
	<br>
	<<textbox "$saveName" $saveName>>
	/*
	<div class="description">세이브 메뉴에서 세이브 id 자리에 표시됩니다.</div>
	<div class="description"><span class="red">현재 세이브 메뉴 상에서는 최대 10글자까지만 표시가 되고, 세이브 ID를 표시하려면 공란으로 남겨두세요</span></div>

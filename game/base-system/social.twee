			<<else>>
				<<set $_name to "엄마">>
			<</if>>
			<<set _pregnancyFameConfig = 		{ currentValue: $famepregnancy,		preText: '임신:',		flavorText: $_name,				states: _fameStates }>>
			<<set _combatFameConfig =			{ currentValue: $famescrap,			preText: '전투:',		flavorText: '싸움꾼',			states: _inverseFameStates }>>

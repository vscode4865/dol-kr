/*args[0] - childId*/
<<widget "childtype">><<silently>>
	<<if _args[0] isnot undefined>>
		<<set $_id to _args[0]>>
	<<elseif $childSelected isnot undefined>>
		<<set $_id to $childSelected.childId>>
	<</if>>
	<<if $_id isnot undefined>>
		<<switch $children[$_id].type>>
			<<case "human">><<set $_text_output to "아이">>
			<<case "wolf" "wolfboy" "wolfgirl">><<set $_text_output to "새끼 늑대">>
			<<case "hawk">><<set $_text_output to ($children[$_id].features.monster is "monster" ? "매 아이" : "새끼 새")>>
		<</switch>>
	<</if>>
<</silently>><<if $_text_output>><<print $_text_output>><</if>><</widget>>

/*args[0] - child object*/
<<widget "childTypeDisplay">><<silently>>	/* ToDo: 필요시 번역위젯 만들것 */
	<<if _args[0] and _args[0].childId>>
		<<if _args[0].type isnot "human">>
			<<if _args[0].features.monster>>
				<<switch _args[0].type>>
					<<case "wolf">>
						<<set $_text_output to "늑대">>
						<<switch _args[0].gender>>
							<<case "m">><<set $_text_output += " 소년">>
							<<case "f">><<set $_text_output += " 소녀">>
							<<default>><<set $_text_output += " 아이">>
						<</switch>>
					<<case "hawk">>
						<<set $_text_output to "하피">>
				<</switch>>
			<<else>>
				<<trBeasttypeFun _args[0].type>><<set $_text_output to _trResult>>
			<</if>>
		<<else>>
			<<set $_text_output to "인간 <<if _args[0].features.beastTransform or _args[0].features.divineTransform>> (<<childTransform _args[0]>>)<</if>>">>
		<</if>>
	<</if>>
<</silently>><<if $_text_output>>종류: <<print $_text_output>> |<</if>>
<</widget>>

<<widget "childTransform">><<silently>>	/* ToDo: 필요시 번역위젯 만들것 */
	<<if _args[0] and _args[0].childId>>
		<<set $_child to _args[0]>>
	<<elseif _args[0] isnot undefined>>
		<<set $_id to _args[0]>>
		<<set $_child to $children[$_id]>>
	<<elseif $childSelected isnot undefined>>
		<<set $_id to $childSelected.childId>>
		<<set $_child to $children[$_id]>>
	<</if>>
	<<if $_child isnot undefined>>
		<<set $_text_output to "">>
		<<switch $_child.features.divineTransform>>
			<<case "angel">><<set $_text_output += "천사">>
			<<case "demon">><<set $_text_output += "악마">>
			<<case "fallen">><<set $_text_output += "타천사">>
		<</switch>>
		<<if $_child.features.divineTransform and $_child.features.beastTransform>><<set $_text_output += " ">><</if>>
		<<switch $_child.features.beastTransform>>
			<<case "cat">><<set $_text_output += "고양이" + ($_child.gender === "m" ? " 소년" : " 소녀")>>
			<<case "cow">><<set $_text_output += ($_child.gender === "m" ? "황소 소년" : "암소 소녀")>>
			<<case "wolf">><<set $_text_output += "늑대" + ($_child.gender === "m" ? " 소년" : " 소녀")>>
			<<case "bird">><<set $_text_output += "새" + ($_child.gender === "m" ? " 소년" : " 소녀")>>
			<<case "fox">><<set $_text_output += "여우" + ($_child.gender === "m" ? " 소년" : " 소녀")>>
		<</switch>>
	<</if>>
<</silently>><<if $_text_output>><<print $_text_output>><</if>><</widget>>

/*args[0] - childId, args[1] - word*/
<<widget "childFirstWord">><<silently>>
	<<if _args[0] isnot undefined>>
		<<set $_id to _args[0]>>
	<<elseif $childSelected isnot undefined>>
		<<set $_id to $childSelected.childId>>
	<</if>>
	<<if $_id isnot undefined and !$children[$_id].localVariables.firstWord>>
		<<set $_text_output to $children[$_id].localVariables.firstWord.word>>
	<</if>>
<</silently>><<if $_text_output>><<print $_text_output>><</if>><</widget>>

/*args[0] - location, args[1] - toyid, toy set or toy name, args[2] - option of either "notext" or "lowercase"*/
<<widget "childtoy">><<silently>>
	<<set _toys to $storedChildrenToys[_args[0]]>>
	<<if _toys>>
		<<if between(_args[1], 0, _toys.length - 1)>>
			<<set _toy to _toys[_args[1]]>>
		<<elseif _args[1]>>
			<<set _toySelect to []>>
			<<for $_i to 0; $_i lt _toys.length; $_i++>>
				<<if _toys[$_i].set is _args[1] or _toys[$_i].name is _args[1]>>
					<<run _toySelect.push($_i)>>
				<</if>>
			<</for>>
			<<if _toySelect.length gt 0>>
				<<set _toyid to _toySelect[random(0,_toySelect.length - 1)]>>
				<<set _toy to _toys[_toyid]>>
			<</if>>
		<<else>>
			<<set _toyid to random(0,_toys.length - 1)>>
			<<set _toy to _toys[_toyid]>>
		<</if>>
		<<if _toy>>
			<<switch _args[2]>>
				<<case "notext">>
				<<case "lowercase">>
					<<set $_text_output to clone(_toy.name_lower)>>
					<<set _toyName to clone(_toy.name_lower)>>
				<<default>>
					<<set $_text_output to clone(_toy.name)>>
					<<set _toyName to clone(_toy.name)>>
			<</switch>>
		<</if>>
	<</if>>
<</silently>><<if $_text_output>><<print $_text_output>><</if>><</widget>>

/*args[0] - location*/
<<widget "childSelectRandom">>
	<<set $childSelected to Object.values($children).filter(child => !_args[0] or child.location === _args[0]).random()>>
<</widget>>

<<widget "childViewerDisplay">>
	<<set _idList to _args[0]>>
	<<set _listMaxItems to 10>>
	<<set _listPage to 1>>
	<<set _listPageMax to Math.ceil(_idList.length / _listMaxItems)>>
	<<set _childStart to 0>>
	<<set _childEnd to (_childStart + _listMaxItems gt _idList.length ? _idList.length : _childStart + _listMaxItems)>>
	<div id="childViewer">
		<<childViewerDisplayElements>>
	</div>
<</widget>>

<<widget "childViewerPageUpdate">>
	<<set _childStart to _listMaxItems * (_listPage - 1)>>
	<<set _childEnd to (_childStart + _listMaxItems lte _idList.length ? _childStart + _listMaxItems : _idList.length)>>
	<<replace '#childViewer'>>
		<<childViewerDisplayElements>>
	<</replace>>
<</widget>>

<<widget "childViewerDisplayElements">>
	<div class="childrenUi">
		<<for $_i to _childStart; $_i lt _childEnd; $_i++>>
			<<set _childId to _childrenIds[$_i]>>
			<<capture _childId>>
				<div @id="'child-' + _childId">
					<<childViewerElement `_childId`>>
				</div>
			<</capture>>
		<</for>>
	</div>
	<br>
	<div class="childrenUiControls">
		<<set _disabled = _listPage > 1 ? "" : "disabled">>
		<div @class="'div-link btn-pagination prev ' + _disabled">
			<<link "이전">>
				<<if _listPage > 1>>
					<<set _listPage -= 1>>
					<<childViewerPageUpdate>>
				<</if>>
			<</link>>
			<div class="btn-pagination-arrow">&lt;</div>
		</div>
		<div>
			<<print (_listPage) + " / " + _listPageMax>>
		</div>
		<<set _disabled = _listPage < _listPageMax ? "" : "disabled">>
		<div @class="'div-link btn-pagination next ' + _disabled">
			<<link "다음">>
				<<if _listPage < _listPageMax>>
					<<set _listPage += 1>>
					<<childViewerPageUpdate>>
				<</if>>
			<</link>>
			<div class="btn-pagination-arrow">&gt;</div>
		</div>
	</div>
	<<run linkifyDivs('#childViewer')>>
<</widget>>

<<widget "childViewerElement">>
	<<if _children is undefined>>
		<<set _children to {}>>
	<</if>>
	<<capture _args>>
		<<childTypeDisplay $children[_args[0]]>>
		성별:
		<<switch $children[_args[0]].gender>>
			<<case "m">>남성
			<<case "f">>여성
			<<case "h">>후타나리
		<</switch>>
		<br>
		<label>이름: <<textbox "_children[_args[0]]" `$children[_args[0]].name`>></label>
		<br>
		<<childActivity _args[0]>>
		<<childViewerHiddenElements _args[0] _args[1]>>
	<</capture>>
<</widget>>

<<widget "childViewerHiddenElements">>
	<<if _args[0]>>
		<div @id="'childViewerHidden-'+_args[0]" @class="'childViewerHidden' + (_args[1] ? '' : ' hidden')">
			어머니:
			<<if $children[_args[0]].motherKnown>>
				<<print pregnancyNameCorrection($children[_args[0]].mother, true)>>
			<<else>>
				????
			<</if>>
			| 아버지:
			<<if $children[_args[0]].fatherKnown>>
				<<print pregnancyNameCorrection($children[_args[0]].father, true)>>
			<<else>>
				????
			<</if>>
			<br>
			<<if $children[_args[0]].laid>>
				낳은 날: <<trMonth $children[_args[0]].laid.month>><<print $children[_args[0]].laid.year + "년 " + _trResult + " " + $children[_args[0]].laid.day + "일">> |
				부화한 날: <<trMonth $children[_args[0]].born.month>><<print $children[_args[0]].born.year + "년 " + _trResult + " " + $children[_args[0]].born.day + "일">> |
			<<else>>
				태어난 날: <<trMonth $children[_args[0]].born.month>><<print $children[_args[0]].born.year + "년 " + _trResult + " " + $children[_args[0]].born.day + "일">> |
			<</if>>
			<<if $children[_args[0]].adopted>>
				입양한 날: <<trMonth $children[_args[0]].adopted.month>><<print $children[_args[0]].adopted.year + "년 " + _trResult + " " + $children[_args[0]].adopted.day + "일">> |
			<</if>>
			<br>
			나이: <<childAge _args[0] "full">>
			<br>
			몸집: <<trBodysize $children[_args[0]].features.size>><<print _trResult>> | 머리색: <<hairmapcolourtext $children[_args[0]].features.hairColour>>
			<<if $children[_args[0]].features.skinColour>>
				| 피부색: <<skinColourName $children[_args[0]].features.skinColour>>
			<</if>>
			<br>
			<<if $children[_args[0]].features.identical>>
				일란성 쌍둥이: <<print ($children[_args[0]].features.identical ? "네" : "아니오")>>
				<br>
			<</if>>
			<<if $children[_args[0]].localVariables.interactionsTotal>>
				총 의사소통 횟수: <<print $children[_args[0]].localVariables.interactionsTotal>>회
				<br>
			<</if>>
			<span class="no-numberify">
			<<link "이름 변경">>
				<<set $children[_args[0]].name to generateBabyName(_children[_args[0]], $children[_args[0]].gender, _args[0])>>
				<<replace `"#child-" + _args[0]`>><<childViewerElement _args[0] true>><</replace>>
			<</link>> |
			<<link "무작위 이름">>
				<<set $children[_args[0]].name to generateBabyName(undefined, $children[_args[0]].gender, _args[0])>>
				<<replace `"#child-" + _args[0]`>><<childViewerElement _args[0] true>><</replace>>
			<</link>>
			</span>
			<br>
			<small class="right">
				아기 Id: <<print $children[_args[0]].childId>>.
				세이브 Id: <<print $saveId>>.
				<<if $saveName isnot "">>
					| 세이브 이름: <<print $saveName>>.
				<</if>>
			</small>
		</div>
	<</if>>
<</widget>>

<<widget "childViewerHiddenElementsUnhide">>
	<<capture _args>>
		<<if _args[0]>>
			<<toggleclass `'#childViewerHidden-' + _args[0]` "hidden">>
		<</if>>
	<</capture>>
<</widget>>

<<widget "childActivity">>
	<<if _args[0]>>
		<<childAge _args[0]>>
		<<childSelect _args[0]>>
		/*'updateChildActivity' Changes the current event if enough time has passed*/
		<<updateChildActivity _args[0]>>
		<<switch $children[_args[0]].type>>
			<<case "human">><<humanChildActivity _args[0]>>
			<<case "wolf" "wolfboy" "wolfgirl">><<wolfChildActivity _args[0]>>
			<<case "hawk">><<hawkChildActivity _args[0]>>
		<</switch>>
		<span class="no-numberify">
			<<link "추가 사항">><<childViewerHiddenElementsUnhide _args[0]>><</link>>
		</span>
	<</if>>
<</widget>>

/*ToDo: Pregnancy Story Content: Lacking events. Human children events which the place accesses by going to the childrens UI*/
<<widget "humanChildActivity">>
	<<set _activityColour to !$daily.childInteractions or !$daily.childInteractions.includes(_args[0]) ? " childrensActivityGold" : "">>
	<span @class="'no-numberify' + _activityColour">
		<<switch $children[_args[0]].localVariables.activity>>
			<<case "noEvent">>
				<<childHe_ ga>> 고요하게 잠들어 있다.
			<<case "sleeping">>
				<<childHe_ ga>> 고요하게 잠들어 있다.
				<<if $children[_args[0]].localVariables.event>>
					<br>
					<<set _rng to random(1, 6)>>
					<<switch _rng>>
						<<case 1>>
							<<link [[<<childhim_ ul>> 지켜본다 (0:05)|Children Activity Events]]>><<pass 5>><<set $childActivityEvent to {event:"transfixed", childid:_args[0]}>><</link>> |
						<<case 2>>
							<<link [[<<childhim_ ul>> 지켜본다 (0:05)|Children Activity Events]]>><<pass 5>><<set $childActivityEvent to {event:"stroke", childid:_args[0]}>><</link>> |
						<<case 3>>
							<<link [[<<childhim_ ul>> 지켜본다 (0:05)|Children Activity Events]]>><<pass 5>><<set $childActivityEvent to {event:"yawn", childid:_args[0]}>><</link>> |
						<<case 4>>
							<<link [[<<childhim_ ul>> 지켜본다 (0:05)|Children Activity Events]]>><<pass 5>><<set $childActivityEvent to {event:"murmur", childid:_args[0]}>><</link>> |
						<<case 5>>
							<<link [[잠시 눈을 붙인다 (0:30)|Children Activity Events]]>><<pass 30>><<set $childActivityEvent to {event:"cotNap", childid:_args[0]}>><</link>> |
						<<default>>
							<<link [[<<childhim_ ul>> 지켜본다 (0:05)|Children Activity Events]]>><<pass 5>><<set $childActivityEvent to {event:"shiverInCot", childid:_args[0]}>><</link>> |
					<</switch>>
				<</if>>
			<<case "restlessSleep">>
				<<childHe_ ga>> 잠든 채로 계속 뒤척인다.
				<<if $children[_args[0]].localVariables.event>>
					<br>
					<<link [[<<childhim_ ul>> 진정시킨다 (0:30)|Children Activity Events]]>><<pass 30>><<set $childActivityEvent to {event:"restlessSleep", childid:_args[0]}>><</link>> |
				<</if>>
			<<case "crying">>
				<<childHe_ ga>> 괴로워하며 울고 있다.
				<<if $children[_args[0]].localVariables.event>>
					<br>
					<<set _rng to random(1, 6)>>
					<<switch _rng>>
						<<case 1>>
							<<link [[안심시킨다 (0:20)|Children Activity Events]]>><<pass 20>><<set $childActivityEvent to {event:"holdToShoulder", childid:_args[0]}>><</link>> |
						<<case 2>>
							<<link [[안심시킨다 (0:20)|Children Activity Events]]>><<pass 20>><<set $childActivityEvent to {event:"melodicLullaby", childid:_args[0]}>><</link>> |
						<<case 3>>
							<<link [[안심시킨다 (0:20)|Children Activity Events]]>><<pass 20>><<set $childActivityEvent to {event:"foreheadKiss", childid:_args[0]}>><</link>> |
						<<case 4>>
							<<link [[안심시킨다 (0:20)|Children Activity Events]]>><<pass 20>><<set $childActivityEvent to {event:"objectShow", childid:_args[0]}>><</link>> |
						<<case 5>>
							<<link [[안심시킨다 (0:20)|Children Activity Events]]>><<pass 20>><<set $childActivityEvent to {event:"carryRevolt", childid:_args[0]}>><</link>> |
						<<default>>
							<<link [[안심시킨다 (0:20)|Children Activity Events]]>><<pass 20>><<set $childActivityEvent to {event:"rockToSleep", childid:_args[0]}>><</link>> |
					<</switch>>
				<</if>>
			<<case "lonely">>
				<<childHe_ ga>> 외로워 보인다.
				<<if $children[_args[0]].localVariables.event>>
					<br>
					<<set _rng to random(1, 10)>>
					<<switch _rng>>
						<<case 1>>
							<<link [[달래준다 (0:20)|Children Activity Events]]>><<pass 20>><<set $childActivityEvent to {event:"nurseryRhymes", childid:_args[0]}>><</link>> |
						<<case 2>>
							<<link [[달래준다 (0:20)|Children Activity Events]]>><<pass 20>><<set $childActivityEvent to {event:"FeetCup", childid:_args[0]}>><</link>> |
						<<case 3>>
							<<link [[달래준다 (0:20)|Children Activity Events]]>><<pass 20>><<set $childActivityEvent to {event:"shareThoughts", childid:_args[0]}>><</link>> |
						<<case 4>>
							<<link [[달래준다 (0:20)|Children Activity Events]]>><<pass 20>><<set $childActivityEvent to {event:"speakCry", childid:_args[0]}>><</link>> |
						<<case 5>>
							<<link [[달래준다 (0:20)|Children Activity Events]]>><<pass 20>><<set $childActivityEvent to {event:"waveTongue", childid:_args[0]}>><</link>> |
						<<case 6>>

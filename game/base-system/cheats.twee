			<<set $player.virginity.vaginal to true>>
		<</if>>
		<<trVirginity $player.virginity.vaginal>><<replace "#statsvaginalvirginity">><<print ($player.virginity.vaginal is true ? "존재" : "가져간 사람: " + ($player.virginity.vaginal is false ? "불명" : _trResult))>><</replace>>
	<</link>>
	<br>
<</if>>

<<if $player.penisExist>>

:: widget clothesPost [widget nobr]
<<widget "undertopPost">><<silently>>
    <<if $worn.under_upper.exposed lte 0>>
        <<trClothes "under_upper" $worn.under_upper.name "name" _args[0] _args[1]>>
    <<else>>
        <<breastsPost _args[0] _args[1]>>
    <</if>>
<</silently>><<print _trResult>><</widget>>

<<widget "groinPost">><<silently>>
	<<genitalsPost _args[0] _args[1]>><<set $_genitals to _trResult>>
	<<outfitChecks>>
	<<if !_botomIsSkirt and _bottom>>
		<<bottomNamePost>>
		<<set _trResult to _trResult + " 너머로 " + $_genitals>>
	<<else>>
		<<set _trResult to $_genitals>>
	<</if>>
<</silently>><<print _trResult>><</widget>>

<<widget "crotchPost">><<silently>>
	<<outfitChecks>>
	<<if _bottom>>
		<<bottomNamePost _args[0] _args[1]>>
	<<else>>
		<<genitalsPost _args[0] _args[1]>>
	<</if>>
<</silently>><<print _trResult>><</widget>>

<<widget "undiesPost">><<silently>>
	<<if _args[0] isnot undefined>>
		<<if typeof _args[0] is "string" and _args[0].substring(0,1).search("[가-힣]") gte 0>>
			<<unset $_num>><<unset $_pluck>><<set $_post to _args[0]>><<set $_sep to _args[1]>>
		<<elseif _args[1] isnot undefined and typeof _args[1] is "string" and _args[1].substring(0,1).search("[가-힣]") gte 0>>
			<<set $_num to _args[0]>><<unset $_pluck>><<set $_post to _args[1]>><<set $_sep to _args[2]>>
		<<else>>
			<<set $_num to _args[0]>><<set $_pluck to _args[1]>><<set $_post to _args[2]>><<set $_sep to _args[3]>>
		<</if>>
	<<else>>
		<<set $_num to _args[0]>><<set $_pluck to _args[1]>><<set $_post to _args[2]>><<set $_sep to _args[3]>>
	<</if>>

	<<outfitChecks>>
	<<if !$worn.under_lower.type.includes("naked")>>
		<<if $_num and $_num gte 1>>
			<<trClothes "under_lower" $worn.under_lower.name_simple "name" $_post $_sep>>
		<<else>>
			<<trClothes "under_lower" $worn.under_lower.name "name" $_post $_sep>>
		<</if>>
    <<elseif !$worn.genitals.type.includes("naked")>>
        <<trClothes "genitals" $worn.genitals.name "name" $_post $_sep>>
    <<else>>
        <<genitalsPost $_num $_post $_sep>>
    <</if>>
<</silently>><<print _trResult>><</widget>>

<<widget "bottomsPost">><<silently>>
	<<outfitChecks>>
	<<set _trResult to bottomNamePost(_args[0], _args[1])>>
<</silently>><<print _trResult>><</widget>>

<<widget "underbottomsPost">><<silently>>
	<<outfitChecks>>
	<<if _underOutfit>>

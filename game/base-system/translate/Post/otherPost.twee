<span class="lewd"><<trBodyWriting $skin[$args[0]].writing>>"<<print _trResult>>"</span>
<<switch $skin[$args[0]].pen>>
	<<case "tattoo">><<set _trResult to "문신">><<trPost 0 $args[1] $args[2]>>
	<<case "brand">><<set _trResult to "낙인">><<trPost 0 $args[1] $args[2]>>
	<<case "magic">><<set _trResult to "문양">><<trPost 0 $args[1] $args[2]>>
	<<default>>
		<<if $skin[$args[0]].type is "text">>
			<<set _trResult to "글씨">><<trPost 1 $args[1] $args[2]>>
		<<else>>
			<<set _trResult to "그림">><<trPost 0 $args[1] $args[2]>>
		<</if>>
<</switch>>
<</silently>><<print _trResult>><</widget>>

<<widget "plant_detailsPost">><<silently>>
<<if $args[0] is undefined or $args[0].substring(0,1).search("[가-힣]") gte 0>>
	<<unset $_arg>><<set $_post to $args[0]>><<set $_sep to $args[1]>>
<<else>>
	<<set $_arg to $args[0]>><<set $_post to $args[1]>><<set $_sep to $args[2]>>
<</if>>
<<if ($location is "moor" and $_arg isnot "forest") or $_arg is "moor">>
	<<switch $season>>
		<<case "spring">>
			<<set _trResult to "노란 버드나무 이파리들">>
		<<case "summer">>
			<<set _trResult to "작은 보라색 꽃들">>
		<<case "autumn">>
			<<set _trResult to "커다란 호박색 꽃들">>
		<<case "winter">>
			<<set _trResult to "지푸라기 색의 갈대들">>
	<</switch>>
<<else>>
	<<switch $season>>
		<<case "spring">>
			<<set _trResult to "분홍색 꽃들">>
		<<case "summer">>
			<<set _trResult to "이파리와 꽃들">>
		<<case "autumn">>
			<<set _trResult to "주황색과 빨강색의 이파리들">>
		<<case "winter">>
			<<set _trResult to "작은 빨간 산딸기들">>
	<</switch>>
<</if>>
<<trPost 2 $_post $_sep>>
<</silently>><<print _trResult>><</widget>>

<<widget "Plant_detailsPost">><<plant_detailsPost $args[0]>><</widget>>

<<widget "tower_creature_textPost">><<silently>>
	<<if $NPCList[0].monster is "monster">>
		<<set _trResult to "뿔이 난 ">>
		<<if $NPCList[0].gender is "m">>
			<<set _trResult += "소년">><<trPost 0 $args[0] $args[1]>>
		<<else>>
			<<set _trResult += "소녀">><<trPost 1 $args[0] $args[1]>>
		<</if>>
	<<else>>
		<<set _trResult to either("생물체","괴물","짐승")>>
		<<if $args[0] isnot undefined>>
			<<getPostNum _trResult>>
			<<trPost _postNum $args[0] $args[1]>>
		<</if>>
	<</if>>
<</silently>>	<<print _trResult>><</widget>>
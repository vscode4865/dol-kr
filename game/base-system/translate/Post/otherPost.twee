<<widget "farm_text_manyPost">><<silently>>
<<set $_bgender to $farm_work[$args[0]].gender_plural>>
<<set $_bperson to ($farm_work[$args[0]].monster_roll is true and ($hallucinations gte 1 or $monsterhallucinations is "f"))>>
<<if $args[0] is "horse">>
	<<if $_bperson>>
		<<set _trResult to "켄타우로스들">>
	<<else>>
		<<set _trResult to "말들">>
	<</if>>
<<elseif $args[0] is "pig">>
	<<if $_bperson>>
		<<if $_bgender is "m">>
			<<set _trResult to "돼지 소년들">>
		<<elseif $_bgender is "f">>
			<<set _trResult to "돼지 소녀들">>
		<<else>>
			<<set _trResult to "돼지 소년 소녀들">>
		<</if>>
	<<else>>
		<<set _trResult to "돼지들">>
	<</if>>
<<elseif $args[0] is "dog">>
	<<if $_bperson>>
		<<if $_bgender is "m">>
			<<set _trResult to "개 소년들">>
		<<elseif $_bgender is "f">>
			<<set _trResult to "개 소녀들">>
		<<else>>
			<<set _trResult to "개 소년 소녀들">>
		<</if>>
	<<else>>
		<<set _trResult to "개들">>
	<</if>>
<<else>>
	<<if $_bperson>>
		<<if $_bgender is "m">>
			<<set _trResult to "황소 소년들">>
		<<elseif $_bgender is "f">>
			<<set _trResult to "젖소 소녀들">>
		<<else>>
			<<set _trResult to "젖소 소녀들과 황소 소년들">>
		<</if>>
	<<else>>
		<<set _trResult to "소들">>
	<</if>>
<</if>>
<<trPost 2 $args[1] $args[2]>>
<</silently>><<print _trResult>><</widget>>



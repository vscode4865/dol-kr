:: Scripts bodyPost [script]

function handPost(hand, post, sep)
{
	let tempArray = [];
	let both = false;
	if (hand)
	{
		if (hand.substring(0,1).search("[가-힣]") >= 0)
		{
			sep = post;
			post = hand;
		}
		else
		{
			tempArray = hand.split(' ');

			hand = tempArray.pop();
			if (hand.includes("hand"))	/* ex. "left hand" */
			{
				if (hand == "hands")
					both = true;

				hand = tempArray.pop();
			}
		}
	}
	if (hand == "left")
		tempArray.push("왼손");
	else if (hand == "right")
		tempArray.push("오른손");
	else if (hand == "both" || both == true)
		tempArray.push("양손");
	else
		tempArray.push("손");
	
	T.trResult = tempArray.join(" ");
    if (post)
        trPost(0, post, sep);
    
	return T.trResult;
}
window.handPost = handPost;
DefineMacroS("handPost", handPost);